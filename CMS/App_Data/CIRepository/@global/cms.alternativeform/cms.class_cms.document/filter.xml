        <properties>
          <fieldcaption>{$pages.filter.Persona$}</fieldcaption>
          <fielddescription>Filter by personas assigned to a page.</fielddescription>
          <visiblemacro ismacro="true">
            <![CDATA[{%IsModuleLoaded("CMS.Personas") && LicenseHelper.CheckFeature(FeatureEnum.Personas)|(identity)GlobalAdministrator|(hash)b16351a712139f975ba97366a3f78e7655cd815f70f975354559344957cbe23c%}]]>
          </visiblemacro>
        </properties>
        <settings>
          <AddGlobalObjectNamePrefix>False</AddGlobalObjectNamePrefix>
          <AddGlobalObjectSuffix>False</AddGlobalObjectSuffix>
          <AllowAll>True</AllowAll>
          <AllowDefault>False</AllowDefault>
          <AllowEditTextBox>False</AllowEditTextBox>
          <AllowEmpty>True</AllowEmpty>
          <controlname>personaselector</controlname>
          <DialogWindowHeight>590</DialogWindowHeight>
          <DialogWindowName>SelectionDialog</DialogWindowName>
          <DialogWindowWidth>668</DialogWindowWidth>
          <EditDialogWindowHeight>700</EditDialogWindowHeight>
          <EditDialogWindowWidth>1000</EditDialogWindowWidth>
          <EditWindowName>EditWindow</EditWindowName>
          <EncodeOutput>True</EncodeOutput>
          <GlobalObjectSuffix>(global)</GlobalObjectSuffix>
          <ItemsPerPage>25</ItemsPerPage>
          <LocalizeItems>True</LocalizeItems>
          <MaxDisplayedItems>25</MaxDisplayedItems>
          <MaxDisplayedTotalItems>50</MaxDisplayedTotalItems>
          <ObjectSiteName>#currentsite</ObjectSiteName>
          <ObjectType>Personas.Persona</ObjectType>
          <OrderBy>PersonaDisplayName</OrderBy>
          <RemoveMultipleCommas>False</RemoveMultipleCommas>
          <ReturnColumnName>PersonaID</ReturnColumnName>
          <ReturnColumnType>id</ReturnColumnType>
          <SelectionMode>1</SelectionMode>
          <UseAutocomplete>False</UseAutocomplete>
          <ValuesSeparator>;</ValuesSeparator>
          <WhereConditionFormat>
<![CDATA[
({1} = -1)
OR 
({1} = 0 AND NOT EXISTS (SELECT * FROM (SELECT NodeID AS Personas_PersonaNodeNodeID FROM Personas_PersonaNode) TPersonas_PersonaNode WHERE Personas_PersonaNodeNodeID = NodeID))
OR
({1} > 0 AND NodeID IN (SELECT NodeID FROM Personas_PersonaNode WHERE PersonaID={1}))
]]>
          </WhereConditionFormat>
        </settings>
      </field>
