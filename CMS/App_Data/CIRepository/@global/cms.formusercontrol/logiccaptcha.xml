<?xml version="1.0" encoding="utf-8"?>
<cms.formusercontrol>
  <UserControlCodeName>LogicCaptcha</UserControlCodeName>
  <UserControlDefaultDataType>Text</UserControlDefaultDataType>
  <UserControlDefaultDataTypeSize>200</UserControlDefaultDataTypeSize>
  <UserControlDescription>
    <![CDATA[Provides a text or image CAPTCHA test. The options allow setting statements (pairs of questions and correct answers), mathemtatical operations or questions for generated images.]]>
  </UserControlDescription>
  <UserControlDisplayName>Logic CAPTCHA</UserControlDisplayName>
  <UserControlFileName>~/CMSFormControls/Captcha/LogicCaptcha.ascx</UserControlFileName>
  <UserControlForBinary>False</UserControlForBinary>
  <UserControlForBoolean>False</UserControlForBoolean>
  <UserControlForDateTime>False</UserControlForDateTime>
  <UserControlForDecimal>False</UserControlForDecimal>
  <UserControlForDocAttachments>False</UserControlForDocAttachments>
  <UserControlForDocRelationships>False</UserControlForDocRelationships>
  <UserControlForFile>False</UserControlForFile>
  <UserControlForGuid>False</UserControlForGuid>
  <UserControlForInteger>False</UserControlForInteger>
  <UserControlForLongText>False</UserControlForLongText>
  <UserControlForText>True</UserControlForText>
  <UserControlForVisibility>False</UserControlForVisibility>
  <UserControlGUID>a2f24d25-5c08-4e66-b2d8-466ca8fdc8c5</UserControlGUID>
  <UserControlIsSystem>False</UserControlIsSystem>
  <UserControlParameters>
    <form version="2">
      <category name="General">
        <properties>
          <visible>True</visible>
        </properties>
      </category>
      <field allowempty="true" column="UseTextOrImageCaptcha" columntype="boolean" displayinsimplemode="true" guid="6c6cc322-0889-4de1-b9c1-bcfb4fdb7e63" publicfield="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>false</defaultvalue>
          <fieldcaption>Type of CAPTCHA</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates if the text or image CAPTCHA type should be used.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>radiobuttonscontrol</controlname>
          <Options>
<![CDATA[
0;Text
1;Image
]]>
          </Options>
          <RepeatDirection>horizontal</RepeatDirection>
        </settings>
      </field>
      <field allowempty="true" column="ShowInfoLabel" columntype="boolean" guid="dafa9c61-fb4c-45db-97de-d49232bc0ed2" publicfield="false" visibility="none">
        <properties>
          <defaultvalue>true</defaultvalue>
        </properties>
      </field>
      <field allowempty="true" column="ShowAfterText" columntype="boolean" guid="0bad9ec0-b798-4ccf-8932-7a88bfcbeafd" publicfield="false" visibility="none">
        <properties>
          <defaultvalue>false</defaultvalue>
          <fieldcaption>Show after text</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates whether an info text should be displayed to the right of the CAPTCHA.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <category name="Methematical operations">
        <properties>
          <caption>Mathematical operations</caption>
          <visible>True</visible>
        </properties>
      </category>
      <field allowempty="true" column="UseMathOperations" columntype="boolean" guid="9deff6c4-70b5-492e-a26e-d60aa17d02d2" publicfield="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>true</defaultvalue>
          <fieldcaption>Use basic mathematical operations</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates if the CAPTCHA should use basic mathematical operations (addition and subtruction). Applicable only if the Text CAPTCHA option is used.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field allowempty="true" column="UseCompareOperations" columntype="boolean" guid="d8ce70d5-cbb6-4cee-b074-07ec7c677500" publicfield="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>true</defaultvalue>
          <fieldcaption>Use compare operations</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates if text CAPTCHA should use compare operations. Applicable only if the Text CAPTCHA option is used.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field allowempty="true" column="UseMultAndDiv" columntype="boolean" guid="1ce52df0-d97b-4eed-bea4-96ae295bdaf3" publicfield="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>false</defaultvalue>
          <fieldcaption>Use mutliplication and dividing</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates if the CAPTCHA should use multiplication and division operations. Applicable only if the Text CAPTCHA option is used.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field allowempty="true" column="ConvertOperandsToText" columntype="boolean" guid="2ca02110-78bf-4e07-8e72-2bc07fea4777" publicfield="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>true</defaultvalue>
          <fieldcaption>Convert operands to text</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates if operands of mathematical operations should be converted to text.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field allowempty="true" column="ConvertOperatorToText" columntype="boolean" guid="44a7b398-1834-4920-ae27-629dc52c1fa3" publicfield="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>false</defaultvalue>
          <fieldcaption>Convert operator to text</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates if operators of mathematical operations should be converted to text.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <category name="Statements">
        <properties>
          <visible>True</visible>
        </properties>
      </category>
      <field allowempty="true" column="UseStatements" columntype="boolean" guid="84a5454e-3fd0-45dd-a46a-e707935ae1e7" publicfield="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>false</defaultvalue>
          <fieldcaption>Use statements</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates if text CAPTCHA should use statements defined by means of the StatementQuestions property. Applicable only if the Text CAPTCHA option is used.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field allowempty="true" column="StatementQuestions" columntype="longtext" guid="8ea63510-44db-4cd9-897f-1fd7f072def5" publicfield="false" visibility="none" visible="true">
        <properties>
          <defaultvalue ismacro="true">
<![CDATA[
{$captcha.sunblack$};0
{$captcha.sunyellow$};1
{$captcha.yellowcolorsame$};0
{$captcha.yellowcolordifferent$};1
{$captcha.grassgreen$};1
{$captcha.grassred$};0
{$captcha.catisanimal$};1
{$captcha.catisnotanimal$};0
{$captcha.humanyes$};0
]]>
          </defaultvalue>
          <fieldcaption>Questions</fieldcaption>
          <fielddescription>
<![CDATA[
Enter definitions of logical questions, one per line.
Format:
text or macro;value
for example:
{$captcha.sunblack$};0
Sun is black;0
]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>largetextarea</controlname>
        </settings>
      </field>
      <category name="Image">
        <properties>
          <visible>True</visible>
        </properties>
      </category>
      <field allowempty="true" column="ImageWidth" columntype="integer" guid="035cc6d0-1c92-430a-9473-00ce17882071" publicfield="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>80</defaultvalue>
          <fieldcaption>Image width</fieldcaption>
          <fielddescription>
            <![CDATA[Sets the width of CAPTCHA image. Applicable only if the Image CAPTCHA option is used.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="ImageHeight" columntype="integer" guid="2e548ee5-4d03-45b6-a09e-493e62da14ec" publicfield="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>20</defaultvalue>
          <fieldcaption>Image height</fieldcaption>
          <fielddescription>
            <![CDATA[Sets the height of CAPTCHA image. Applicable only if the Image CAPTCHA option is used.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="ImageQuestions" columntype="longtext" guid="6e7366c9-2798-4e02-b18b-6d5f878274bc" publicfield="false" visibility="none" visible="true">
        <properties>
          <defaultvalue ismacro="true">
<![CDATA[
{$captcha.firstnumber$};first
{$captcha.lastnumber$};last
{$captcha.number$};number
{$captcha.firststandnumber$};firstandlast
{$captcha.secondnumber$};second
{$captcha.thirdnumber$};third
{$captcha.fourthnumber$};fourth
{$captcha.fifthhnumber$};fifth
{$captcha.firstandsecond$};firstandsecond
{$captcha.max$};max
{$captcha.min$};min
]]>
          </defaultvalue>
          <fieldcaption>Questions (image)</fieldcaption>
          <fielddescription>
<![CDATA[
Enter the definition of Image questions, one per line.
Format:
text or macro;value
for example:
{$captcha.max$};max
Please type the greatest digit into the text box.;max
]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>largetextarea</controlname>
        </settings>
      </field>
    </form>
  </UserControlParameters>
  <UserControlPriority>0</UserControlPriority>
  <UserControlShowInBizForms>True</UserControlShowInBizForms>
  <UserControlShowInCustomTables>True</UserControlShowInCustomTables>
  <UserControlShowInDocumentTypes>True</UserControlShowInDocumentTypes>
  <UserControlShowInReports>True</UserControlShowInReports>
  <UserControlShowInSystemTables>True</UserControlShowInSystemTables>
  <UserControlShowInWebParts>True</UserControlShowInWebParts>
  <UserControlType>7</UserControlType>
</cms.formusercontrol>