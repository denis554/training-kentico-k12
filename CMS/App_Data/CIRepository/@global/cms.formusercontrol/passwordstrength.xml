<cms.formusercontrol>
  <UserControlCodeName>PasswordStrength</UserControlCodeName>
  <UserControlDefaultDataType>Text</UserControlDefaultDataType>
  <UserControlDefaultDataTypeSize>200</UserControlDefaultDataTypeSize>
  <UserControlDescription>
    <![CDATA[Provides a text field for entering user account passwords. Masks input by displaying dots instead of the typed-in characters. Validates the input according to the site's Password policy settings (Settings -> Security & Membership -> Passwords) and displays an indicator of the password strength.]]>
  </UserControlDescription>
  <UserControlDisplayName>Password strength</UserControlDisplayName>
  <UserControlFileName>
    <![CDATA[~/CMSModules/Membership/FormControls/Passwords/PasswordStrength.ascx]]>
  </UserControlFileName>
