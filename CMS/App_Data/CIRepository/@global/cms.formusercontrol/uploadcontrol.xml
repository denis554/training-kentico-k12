        </properties>
        <settings>
          <controlname>AllowedExtensionsSelector</controlname>
        </settings>
      </field>
      <field allowempty="true" column="Allowed_Extensions" columnsize="250" columntype="text" guid="a8381b9c-6296-414d-b2c3-f859ac65e89e" publicfield="false" spellcheck="false" visibility="none">
        <settings>
