<?xml version="1.0" encoding="utf-8"?>
<cms.transformation>
  <TransformationClassID>
    <CodeName>Chat.Transformations</CodeName>
    <GUID>bf7ea731-502b-43b5-ac6f-153551edb84d</GUID>
    <ObjectType>cms.documenttype</ObjectType>
  </TransformationClassID>
  <TransformationCode>
<![CDATA[
<div class="ChatRoomUser {{if IsOnline}}OnlineUser{{else}}OfflineUser{{/if}}">
  {{if KickUserPerm}}
      <div class="PermaKickButton">
              <a href="#" onclick="${KickUserPerm}">
                      <img src='{%GetImageUrl("CMSModules/CMS_Chat/permakick.png")|(identity)GlobalAdministrator|(hash)61a2eb32f5ced57710a6f019974825479aefcf4206ee710b41ec4d1d6a8fb51f%}' title="{$chat.kickuserperm$}" alt="{$chat.kickuserperm$}" width="12px" height="12px"/>
                  </a>
          </div>
  {{/if}}
  
  {{if KickUser}}
      <div class="KickButton">
              <a href="#" onclick="${KickUser}">
                      <img src='{%GetImageUrl("CMSModules/CMS_Chat/kick.png")|(identity)GlobalAdministrator|(hash)85dc79bec53f52c48341ae7b235be1a06cb4558c604d61b8bf75ce2e9a54c175%}' title="{$chat.kick$}" alt="{$chat.kick$}" width="12px" height="12px"/>
                  </a>
          </div>
  {{/if}}
  
  {{if DeleteAdmin}}
      <div class="DeleteAdminButton">
              <a href="#" onclick="${DeleteAdmin}">
                      <img src='{%GetImageUrl("CMSModules/CMS_Chat/security_agent_delete.png")|(identity)GlobalAdministrator|(hash)6daeccb39ab5810cbed0f81b71a2df2d7b859c6279c0c785017494708996b16f%}' title="{$chat.deleteadmin$}" alt="{$chat.deleteadmin$}" width="12px" height="12px"/>
                  </a>
          </div>
  {{/if}}
  
  {{if AddAdmin}}
      <div class="AddAdminButton">
              <a href="#" onclick="${AddAdmin}">
                      <img src='{%GetImageUrl("CMSModules/CMS_Chat/security_agent_add.png")|(identity)GlobalAdministrator|(hash)5ddc78a6f6a9adb4602103efcfd5c2f81345e1c1ae1869de6568ee0cacd328ec%}' title="{$chat.addadmin$}" alt="{$chat.addadmin$}" width="12px" height="12px"/>
                  </a>
          </div>
  {{/if}}
  
  <div class="ChatRoomUserName"> <!-- Do not change this class name -->
    {{if IsOnline}}
        {{if IsCurrentUser}}<span class="CurrentUser"><strong>${Nickname}</strong></span> 
        {{else}}
            {{if OneOnOneChat}}
                <a href="#" onclick="${OneOnOneChat}" target="_blank">${Nickname}</a>
            {{else}}
                ${Nickname}
            {{/if}}
        {{/if}}
    {{else}}
        <span>
            <a href="#" onclick="return false;" title="{$chat.notpresent$}" style="text-decoration:none; cursor:default">${Nickname}</a>
        </span>
    {{/if}} 
    {{if IsAdmin && !IsCreator}} <span class="UserAdmin"> <img src='{%GetImageUrl("CMSModules/CMS_Chat/security_agent.png")|(identity)GlobalAdministrator|(hash)1a92a113628b7341dd53b2ec0e8275aceb02df42b4e5cf3dab71c72b38353ae0%}' title="{$chat.livesite.admin$}" alt="{$chat.livesite.admin$}" width="12px" height="12px"/></span>{{/if}}
    {{if IsCreator}} <span class="UserCreator"> <img src='{%GetImageUrl("CMSModules/CMS_Chat/security_agent_creator.png")|(identity)GlobalAdministrator|(hash)cec9123aa2cc3e98acad95a6f43863f5242c1615fc88366e4d9a9906d8956fb1%}' title="{$chat.livesite.creator$}" alt="{$chat.livesite.creator$}" width="12px" height="12px"/></span>{{/if}}
  </div>
</div>


]]>
  </TransformationCode>
  <TransformationCSS />
  <TransformationGUID>abfda7d0-4571-4523-bfbd-9183085b58c2</TransformationGUID>
  <TransformationHierarchicalXML />
  <TransformationIsHierarchical>False</TransformationIsHierarchical>
  <TransformationName>CMSChatRoomUser</TransformationName>
  <TransformationType>jquery</TransformationType>
</cms.transformation>