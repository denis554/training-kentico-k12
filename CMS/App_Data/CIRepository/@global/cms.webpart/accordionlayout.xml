<?xml version="1.0" encoding="utf-8"?>
<cms.webpart>
  <WebPartCategoryID>
    <CodeName>Layouts</CodeName>
    <GUID>d5cb0ae0-d055-437e-829f-e2c09a7bdce3</GUID>
    <ObjectType>cms.webpartcategory</ObjectType>
  </WebPartCategoryID>
  <WebPartDescription>
    <![CDATA[This web part provides an accordion layout divided into multiple panes that can be expanded or collapsed. Each pane defines a separate web part zone, so any content may be added into the layout. Users can click on the pane headers to select which one should be displayed.]]>
  </WebPartDescription>
  <WebPartDisplayName>Accordion layout</WebPartDisplayName>
  <WebPartDocumentation>
<![CDATA[
<p>To&nbsp;insert content into the&nbsp;<font size="2">accordion&nbsp;</font>layout, edit the&nbsp;page on the <strong>Design</strong> tab. <font size="2">Here you can&nbsp;select individual&nbsp;panes and add any web parts that you need into the corresponding zones.</font><br />
<br />
For more information, see <a href="~/CMSPages/GetDocLink.ashx?link=layout_webparts" target="_blank">Working with layout web parts</a>.<br />
&nbsp;</p>

]]>
  </WebPartDocumentation>
  <WebPartFileName>Layouts/Accordion.ascx</WebPartFileName>
  <WebPartGUID>d884bb41-3629-472f-aada-f956bb86ba40</WebPartGUID>
  <WebPartIconClass>icon-w-accordion-layout</WebPartIconClass>
  <WebPartName>AccordionLayout</WebPartName>
  <WebPartProperties>
    <form version="2">
      <category name="Pane settings">
        <properties>
          <caption>Layout settings</caption>
          <visible>True</visible>
        </properties>
      </category>
      <field column="Panes" columntype="integer" guid="1428aebd-2f7d-465a-880f-ec75e50432cb" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>2</defaultvalue>
          <fieldcaption>Number of panes</fieldcaption>
          <fielddescription>
            <![CDATA[Sets the number of panes that will be available in the accordion layout.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="PaneHeaders" columntype="longtext" guid="88946760-072a-49a5-8400-e5ac16392e07" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Pane headers</fieldcaption>
          <fielddescription>
            <![CDATA[Specifies the names of the pane headers. Each pane name must be defined on a new line. Any additional panes that are not named will use the following format: "Pane <number>"]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textareacontrol</controlname>
          <FilterMode>False</FilterMode>
          <IsTextArea>True</IsTextArea>
          <Wrap>False</Wrap>
        </settings>
      </field>
      <field allowempty="true" column="ActivePaneIndex" columntype="integer" guid="8e5b519c-8c5f-4e20-bc3c-453dedf90906" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Active pane index</fieldcaption>
          <fielddescription>
            <![CDATA[Sets the index of the pane that will be selected by default when the page is loaded. The index of the first pane is 1.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="RequireOpenedPane" columntype="boolean" guid="62483e9d-3e7a-4a94-a34a-514a94773f24" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>true</defaultvalue>
          <fieldcaption>Require opened pane</fieldcaption>
          <fielddescription>
            <![CDATA[If checked, then at least one pane in the accordion must always be selected (expanded). If disabled and an Active pane index is not specified, the accordion will be completely collapsed by default.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field allowempty="true" column="Width" columnsize="100" columntype="text" guid="0cd55c41-920b-4560-93a5-e1fdc840edff" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Width</fieldcaption>
          <fielddescription>
            <![CDATA[Sets the width of the accordion panes. It must be specified as a CSS style value, e.g. 200px or 50%.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="AllowDesignMode" columntype="boolean" guid="d2882cf0-2c22-404b-affb-f2366ef34e1e" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>true</defaultvalue>
          <fieldcaption>Allow design mode</fieldcaption>
          <fielddescription>
            <![CDATA[{$documentation.webpartproperties.allowdesignmode$}]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <category name="Pane design">
        <properties>
          <visible>True</visible>
        </properties>
      </category>
      <field allowempty="true" column="HeaderCSSClass" columnsize="100" columntype="text" guid="e3676fb9-0542-44c7-be46-f72a8db6da4c" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Header CSS class</fieldcaption>
          <fielddescription>
            <![CDATA[Sets the name of the CSS class from the used stylesheet that will be applied to the pane headers.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="SelectedHeaderCSSClass" columnsize="100" columntype="text" guid="de0c6f3f-ed05-45d0-a921-020894b51f6b" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Selected header CSS class</fieldcaption>
          <fielddescription>
            <![CDATA[Sets the name of the CSS class from the used stylesheet that will be applied to the header of the currently active (expanded) pane.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="ContentCSSClass" columnsize="100" columntype="text" guid="3f25ed7e-9c73-4775-97e3-2fbb546362d4" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Content CSS class</fieldcaption>
          <fielddescription>
            <![CDATA[Sets the name of the CSS class from the used stylesheet that will be applied to the content of all panes.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="FadeTransitions" columntype="boolean" guid="b1dae625-b887-448b-88a5-90ffbdd9f3ad" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>false</defaultvalue>
          <fieldcaption>Fade transitions</fieldcaption>
          <fielddescription>
            <![CDATA[If checked, the content of panes will smoothly fade in or out during a transition. The fade effect is not compatible with the Bing maps web part.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field allowempty="true" column="TransitionDuration" columntype="integer" guid="c0337e3b-142a-488d-aff8-4f640fa64303" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>500</defaultvalue>
          <fieldcaption>Transition duration (ms)</fieldcaption>
          <fielddescription>
            <![CDATA[Specifies the duration of the accordion's transition in milliseconds (i.e. the time it takes to change which pane is expanded).]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
    </form>
  </WebPartProperties>
  <WebPartSkipInsertProperties>True</WebPartSkipInsertProperties>
  <WebPartType>6</WebPartType>
</cms.webpart>