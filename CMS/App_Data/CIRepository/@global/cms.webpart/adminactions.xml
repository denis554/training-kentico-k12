<?xml version="1.0" encoding="utf-8"?>
<cms.webpart>
  <WebPartCategoryID>
    <CodeName>GeneralActions</CodeName>
    <GUID>00086044-1f49-4574-ab89-9cd537147b34</GUID>
    <ObjectType>cms.webpartcategory</ObjectType>
  </WebPartCategoryID>
  <WebPartDefaultValues>
    <form />
  </WebPartDefaultValues>
  <WebPartDescription>
    <![CDATA[Displays a link to the administration interface, and a link that opens the current page for editing. You can set the text of the links a select which links are visible.]]>
  </WebPartDescription>
  <WebPartDisplayName>Admin actions</WebPartDisplayName>
  <WebPartFileName>General/AdminActions.ascx</WebPartFileName>
  <WebPartGUID>41a639f3-49fa-438d-8b64-105ac8548c87</WebPartGUID>
  <WebPartIconClass>icon-w-admin-actions</WebPartIconClass>
  <WebPartName>AdminActions</WebPartName>
  <WebPartProperties>
    <form version="2">
      <category name="General settings">
        <properties>
          <visible>True</visible>
        </properties>
      </category>
      <field column="DisplayOnlyToGlobalAdministrator" columntype="boolean" guid="f6f84bd0-cc68-483a-8382-e24bfaf9f910" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>False</defaultvalue>
          <fieldcaption>Display only to global administrator</fieldcaption>
          <fielddescription>
            <![CDATA[If checked, the web part is only visible for users who have the Global administrator privilege level.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field column="CheckPermissions" columntype="boolean" guid="401fecbd-03bd-4d74-9975-d0e636a8d968" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>False</defaultvalue>
          <fieldcaption>Check permissions</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates whether the web part checks the permissons of users to determine if the links are visible. If checked, the administration interface and editing links are only shown to users who are editors or administrators.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field allowempty="true" column="Separator" columnsize="200" columntype="text" guid="a03971c6-0fa1-4ce6-b832-5f754f51135d" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Separator</fieldcaption>
          <fielddescription>
            <![CDATA[Allows you to place HTML code between individual links.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <category name="Administration">
        <properties>
          <visible>True</visible>
        </properties>
      </category>
      <field allowempty="true" column="DefaultUserName" columnsize="100" columntype="text" guid="3b9b6cad-8be1-4d40-9761-a38c170a58fd" publicfield="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Default user name</fieldcaption>
          <fielddescription>
            <![CDATA[Sets a default user name that the web part pre-fills into the logon form of the administration interface.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field column="ShowCMSDeskLink" columntype="boolean" guid="d2b46bb4-70d6-4ab1-af52-a085ccf713f0" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>True</defaultvalue>
          <fieldcaption>Show link</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates if the web part displays the link to the administration interface.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field column="CMSDeskLinkText" columnsize="1000" columntype="text" guid="ac7c4a44-efae-4281-8808-7cc8a657ff6e" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>Administration</defaultvalue>
          <fieldcaption>Link text</fieldcaption>
          <fielddescription>
            <![CDATA[Sets the text caption of the administration interface link.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="CMSDeskText" columnsize="1000" columntype="text" guid="d81ff7f1-d67d-4f0d-94f6-f893b3e09cb9" publicfield="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Text</fieldcaption>
          <fielddescription>
            <![CDATA[Adds surrounding text for the administration interface link. Enter the {0} formatting expression as a placeholder for the actual link.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <category name="Edit document link">
        <properties>
          <caption>Edit page</caption>
          <visible>True</visible>
        </properties>
      </category>
      <field column="ShowEditDocumentLink" columntype="boolean" guid="f28d37e8-a125-4f8e-ace1-561adfa75656" publicfield="false" resolvedefaultvalue="False" spellcheck="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>False</defaultvalue>
          <fieldcaption>Show link</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates if the web part displays the 'Edit page' link.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>CheckBoxControl</controlname>
        </settings>
      </field>
      <field allowempty="true" column="PreferOnSiteEdit" columntype="boolean" guid="1d6e99f9-0541-44c3-9146-a62ca8ced245" publicfield="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>true</defaultvalue>
          <fieldcaption>Prefer On-site editing</fieldcaption>
          <fielddescription>
            <![CDATA[If checked, the edit link opens the current page in On-site editing mode instead of redirecting to the administration interface. On-site editing also needs to be allowed through the 'Enable On-site editing' setting in Settings -> Content -> Content management.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field column="EditDocumentLinkText" columnsize="200" columntype="text" guid="ca9a142b-aae2-4f59-aebc-9da3f80bfcd7" publicfield="false" resolvedefaultvalue="False" spellcheck="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>Edit page</defaultvalue>
          <fieldcaption>Link text</fieldcaption>
          <fielddescription>
            <![CDATA[Sets the text caption of the 'Edit page' link.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>TextBoxControl</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="EditDocumentText" columnsize="1000" columntype="text" guid="9b5afc00-3d60-41b2-9830-2a73f018df7b" publicfield="false" resolvedefaultvalue="False" visibility="none" visible="true">
        <properties>
          <fieldcaption>Text</fieldcaption>
          <fielddescription>
            <![CDATA[Adds surrounding text for the 'Edit page' link. Enter the {0} formatting expression as a placeholder for the actual link.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>TextBoxControl</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
    </form>
  </WebPartProperties>
  <WebPartSkipInsertProperties>False</WebPartSkipInsertProperties>
  <WebPartType>0</WebPartType>
</cms.webpart>