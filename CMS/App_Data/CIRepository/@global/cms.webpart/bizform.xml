<?xml version="1.0" encoding="utf-8"?>
<cms.webpart>
  <WebPartCategoryID>
    <CodeName>BizForms</CodeName>
    <GUID>0b4a9270-13de-4b08-9ec4-1073fdef9f58</GUID>
    <ObjectType>cms.webpartcategory</ObjectType>
  </WebPartCategoryID>
  <WebPartDescription>
    <![CDATA[The On-line form web part allows you to insert your own on-line form on the page.]]>
  </WebPartDescription>
  <WebPartDisplayName>On-line form</WebPartDisplayName>
  <WebPartDocumentation />
  <WebPartFileName>BizForms/bizform.ascx</WebPartFileName>
  <WebPartGUID>5533a787-1ca9-4ded-8c99-8f10773f597a</WebPartGUID>
  <WebPartIconClass>icon-w-on-line-form</WebPartIconClass>
  <WebPartName>BizForm</WebPartName>
  <WebPartProperties>
    <form version="2">
      <category name="BizForm settings">
        <properties>
          <caption>Form settings</caption>
          <visible>True</visible>
        </properties>
      </category>
      <field column="BizFormName" columnsize="100" columntype="text" guid="e79c26fb-d9eb-41d5-9420-d284c7c7f80c" publicfield="false" resolvedefaultvalue="False" visible="true">
        <properties>
          <fieldcaption>Form name</fieldcaption>
          <fielddescription>Name of the On-line form that should be displayed.</fielddescription>
        </properties>
        <settings>
          <controlname>BizFormSelector</controlname>
          <ShowSiteFilter>True</ShowSiteFilter>
        </settings>
      </field>
      <field allowempty="true" column="SiteName" columnsize="200" columntype="text" external="true" guid="15ec3eee-5c40-4a9f-9c1f-0a16921ccaa3" publicfield="false" visible="true">
        <properties>
          <fieldcaption>Site name</fieldcaption>
          <fielddescription>{$documentation.webpartproperties.sitename$}</fielddescription>
        </properties>
        <settings>
          <controlname>selectsite</controlname>
        </settings>
      </field>
      <field allowempty="true" column="AlternativeFormName" columnsize="200" columntype="text" guid="6aea5d78-c802-4eff-89f0-419c5f054f46" publicfield="false" visible="true">
        <properties>
          <fieldcaption>Alternative form name</fieldcaption>
          <fielddescription>
            <![CDATA[Alternative form full name (ClassName.AlternativeFormName). Alternative form definition and layout are used if specified.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>selectalternativeform</controlname>
        </settings>
      </field>
      <field allowempty="true" column="DefaultFormLayout" columnsize="50" columntype="text" guid="bafa5243-da1d-4df1-b007-18a9679fadfe" publicfield="false" resolvedefaultvalue="False" visible="true">
        <properties>
          <defaultvalue>Standard</defaultvalue>
          <fieldcaption>Default form layout</fieldcaption>
          <fielddescription>
            <![CDATA[{$webpart.onlineform.defaultformlayout.description$}]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>RadioButtonsControl</controlname>
          <Options>
<![CDATA[
Standard;{$formlayoutenum.standard$}
Divs;{$formlayoutenum.divs$}
]]>
          </Options>
          <RepeatDirection>vertical</RepeatDirection>
          <RepeatLayout>Flow</RepeatLayout>
          <SortItems>False</SortItems>
        </settings>
      </field>
      <field column="UseColonBehindLabel" columntype="boolean" guid="8c180d0e-a2cd-4d43-a89c-bdbf0f890fb1" publicfield="false" visible="true">
        <properties>
          <defaultvalue>true</defaultvalue>
          <fieldcaption>Use colon (:) in labels</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates if the colon (:) should be used behind the label text.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field allowempty="true" column="ValidationErrorMessage" columnsize="250" columntype="text" guid="7537e7dd-0f1d-422b-9577-95674620b774" publicfield="false" visible="true">
        <properties>
          <fieldcaption>Validation error message</fieldcaption>
          <fielddescription>
            <![CDATA[This message will be displayed after validation failed. Default error message will be used if this one is not set.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>textboxcontrol</controlname>
        </settings>
      </field>
      <category name="Conversion tracking">
        <properties>
          <visible>True</visible>
        </properties>
      </category>
      <field allowempty="true" column="TrackConversionName" columnsize="400" columntype="text" guid="d1a135f0-9ada-43bc-8df8-f0290a26df57" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Track conversion name</fieldcaption>
          <fielddescription>
            <![CDATA[Selects the conversion that will be logged when a user successfully submits the displayed form.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>conversionselector</controlname>
        </settings>
      </field>
      <field allowempty="true" column="ConversionValue" columntype="double" guid="ec004a76-9875-4063-bc51-2c46342554d9" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Conversion value</fieldcaption>
          <fielddescription>
            <![CDATA[{$documentation.webpartproperties.conversionvalue$}]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
    </form>
  </WebPartProperties>
  <WebPartSkipInsertProperties>False</WebPartSkipInsertProperties>
  <WebPartType>0</WebPartType>
</cms.webpart>