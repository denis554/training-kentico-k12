          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="BreadcrumbsSeparatorRTL" columnsize="200" columntype="text" guid="49bfc8b8-c6b3-4d61-8e37-00f10a4bb931" publicfield="false" visibility="none" visible="true">
