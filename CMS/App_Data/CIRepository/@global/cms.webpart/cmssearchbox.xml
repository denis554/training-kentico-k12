<?xml version="1.0" encoding="utf-8"?>
<cms.webpart>
  <WebPartCategoryID>
    <CodeName>SQLSearch</CodeName>
    <GUID>609568eb-68a4-46ca-be98-1589f5454541</GUID>
    <ObjectType>cms.webpartcategory</ObjectType>
  </WebPartCategoryID>
  <WebPartDescription>
    <![CDATA[Displays a simple text box that allows users to enter a search expression and redirects to a page with the search results. This web part uses the SQL search engine.]]>
  </WebPartDescription>
  <WebPartDisplayName>SQL search box</WebPartDisplayName>
  <WebPartDocumentation />
  <WebPartFileName>Search/cmssearchbox.ascx</WebPartFileName>
  <WebPartGUID>5d4502e8-36ec-4b92-b173-fed9520d946b</WebPartGUID>
  <WebPartIconClass>icon-w-sql-search-box</WebPartIconClass>
  <WebPartName>cmssearchbox</WebPartName>
  <WebPartProperties>
    <form version="2">
      <category name="Search box settings">
        <properties>
          <visible>True</visible>
        </properties>
      </category>
      <field allowempty="true" column="SearchButtonText" columnsize="200" columntype="text" guid="67f9656a-edba-4cc3-98fd-31f6f02a4116" publicfield="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Search button text</fieldcaption>
          <fielddescription>{$documentation.webpartproperties.searchbutton$}</fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="SearchButtonCssClass" columnsize="200" columntype="text" guid="7fbb3987-f2fa-4644-affd-592111275a5e" publicfield="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Search button CSS class</fieldcaption>
          <fielddescription>
            <![CDATA[Assigns a CSS class from the used stylesheet to the search button.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="SearchLabelText" columnsize="200" columntype="text" guid="18b09b62-d7db-4258-933f-00b747a185e9" publicfield="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Search label text</fieldcaption>
          <fielddescription>{$documentation.webpartproperties.searchforlabel$}</fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="SearchLabelCssClass" columnsize="200" columntype="text" guid="1b17ea2e-8ac6-46a4-8a96-950e3485a940" publicfield="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Search label CSS class</fieldcaption>
          <fielddescription>
            <![CDATA[Assigns a CSS class from the used stylesheet to the label displayed before the search box.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="SearchTextboxCssClass" columnsize="200" columntype="text" guid="2f0da9b0-b5e2-4fc7-80b9-83e8b61d957d" publicfield="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Search textbox CSS class</fieldcaption>
          <fielddescription>
            <![CDATA[Assigns a CSS class from the used stylesheet to the textbox where the search expression is entered.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="SearchResultsPageUrl" columnsize="200" columntype="text" guid="13ca20de-6c35-4a46-8e21-65bc05f56022" publicfield="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Search results page URL</fieldcaption>
          <fielddescription>
            <![CDATA[URL of the page where the search results are displayed. This page must contain either the SQL Search results or SQL Search dialog with results web part. If empty, the current page is used.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field column="ShowSearchLabel" columntype="boolean" guid="83ba4578-bf36-4222-84b7-ba7b2b5e703f" publicfield="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>true</defaultvalue>
          <fieldcaption>Show search label</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates if the label before the search box should be displayed.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field allowempty="true" column="SearchMode" columnsize="50" columntype="text" guid="12f3a1b0-741c-494d-a5fd-cfc095ad6aad" publicfield="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Search mode</fieldcaption>
          <fielddescription>{$documentation.webpartproperties.searchmode$}</fielddescription>
        </properties>
        <settings>
          <controlname>dropdownlistcontrol</controlname>
          <Options>
            <![CDATA[<item value="anyword" text="Any word" /><item value="allwords" text="All words" /><item value="exactphrase" text="Exact phrase" />]]>
          </Options>
        </settings>
      </field>
      <field allowempty="true" column="SkinID" columnsize="200" columntype="text" guid="3d35d46f-bf03-4fa5-9e95-3d26d26545c8" publicfield="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Skin ID</fieldcaption>
          <fielddescription>{$documentation.webpartproperties.skinid$}</fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <category name="Button settings">
        <properties>
          <visible>True</visible>
        </properties>
      </category>
      <field column="ShowImageButton" columntype="boolean" guid="59f5e25d-a42f-49a5-a29e-cafb380eaf39" publicfield="false" visible="true">
        <properties>
          <defaultvalue>false</defaultvalue>
          <fieldcaption>Show image button</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates if the search button should be displayed as an image.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field allowempty="true" column="ImageUrl" columnsize="200" columntype="text" guid="f8daf843-5795-404c-b5fe-85f1da7639ad" publicfield="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Button image url</fieldcaption>
          <fielddescription>
            <![CDATA[Specifies the URL of the image used for the button (if the Show image button property is enabled).]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
    </form>
  </WebPartProperties>
  <WebPartSkipInsertProperties>False</WebPartSkipInsertProperties>
  <WebPartType>0</WebPartType>
</cms.webpart>