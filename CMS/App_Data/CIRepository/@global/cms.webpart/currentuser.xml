<?xml version="1.0" encoding="utf-8"?>
<cms.webpart>
  <WebPartCategoryID>
    <CodeName>MembershipLogonRegistration</CodeName>
    <GUID>d01e7e0e-fa1e-4a29-ac4b-940bfcba4c1e</GUID>
    <ObjectType>cms.webpartcategory</ObjectType>
  </WebPartCategoryID>
  <WebPartDescription>
    <![CDATA[Displays the user name and full name of the current user.]]>
  </WebPartDescription>
  <WebPartDisplayName>Current user</WebPartDisplayName>
  <WebPartDocumentation />
  <WebPartFileName>Membership/Logon/currentuser.ascx</WebPartFileName>
  <WebPartGUID>5ab793e0-8a99-4549-8184-dbe7ffed5ee3</WebPartGUID>
  <WebPartIconClass>icon-w-current-user</WebPartIconClass>
  <WebPartName>currentuser</WebPartName>
  <WebPartProperties>
    <form version="2">
      <category name="Display settings">
        <properties>
          <visible>True</visible>
        </properties>
      </category>
      <field column="ShowOnlyWhenAuthenticated" columntype="boolean" guid="0cce800b-c9b0-444f-b211-438462565b6b" publicfield="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>true</defaultvalue>
          <fieldcaption>Show only when authenticated</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates if the text of the web part should only be displayed to authenticated (non-public) users.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field column="ShowLabelText" columntype="boolean" guid="6d99848d-6af0-494f-90e7-a648cf42e8ef" publicfield="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>true</defaultvalue>
          <fieldcaption>Show label text</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates if a label should be displayed before the name of the current user. The text of the label may be entered using the Label text property.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field column="ShowUserFullName" columntype="boolean" guid="1eef2f1d-35de-479a-83db-c8cf0901db4e" publicfield="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>true</defaultvalue>
          <fieldcaption>Show user full name</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates if the user's full name should be shown.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field column="ShowUserName" columntype="boolean" guid="7d59c649-bda8-49fe-a822-91d9cec08633" publicfield="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>true</defaultvalue>
          <fieldcaption>Show user name</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates if the user's name should be shown.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field allowempty="true" column="HideUserNameForExternalUsers" columntype="boolean" guid="4ca8c903-b499-4c34-8d2b-4422dcd0c251" publicfield="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>false</defaultvalue>
          <fieldcaption>Hide user name for external users</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates if the user name should be hidden for external users, i.e. those authenticated against an external database.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field allowempty="true" column="AuthenticatedLinkUrl" columnsize="200" columntype="text" guid="5c03edf6-c19e-4395-9482-904bda916fdd" publicfield="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Authenticated link URL</fieldcaption>
          <fielddescription>
            <![CDATA[If a value is entered, the name of the user will work as a link to the specified URL. This URL will only be used for authenticated users.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="PublicLinkUrl" columnsize="100" columntype="text" guid="acf3f342-e51e-4c5c-9e58-6dd1be61941f" publicfield="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Public link URL</fieldcaption>
          <fielddescription>
            <![CDATA[If a value is entered, the name of the user will work as a link to the specified URL. This URL will only be used for public users.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="LabelText" columnsize="200" columntype="text" guid="86c65048-632f-4523-9bda-8ee7aadb508f" publicfield="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Label text</fieldcaption>
          <fielddescription>
            <![CDATA[Can be used to enter the text of the label displayed before the user's name.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="LabelCSS" columnsize="200" columntype="text" guid="bbbda193-7fa8-4e2c-b41d-0c1de80f66ec" publicfield="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Label CSS class</fieldcaption>
          <fielddescription>
            <![CDATA[Sets the name of the CSS class from the used stylesheet that will be applied to the text of the label displayed before the user's name.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="UserCSS" columnsize="200" columntype="text" guid="b66e9531-2ebd-48bc-bfbd-af1f2004a864" publicfield="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>User text CSS class</fieldcaption>
          <fielddescription>
            <![CDATA[Sets the name of the CSS class from the used stylesheet that will be applied to the text of the user's name.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
    </form>
  </WebPartProperties>
  <WebPartSkipInsertProperties>False</WebPartSkipInsertProperties>
  <WebPartType>0</WebPartType>
</cms.webpart>