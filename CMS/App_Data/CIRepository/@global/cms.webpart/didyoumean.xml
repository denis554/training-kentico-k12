<?xml version="1.0" encoding="utf-8"?>
<cms.webpart>
  <WebPartCategoryID>
    <CodeName>FulltextSearch</CodeName>
    <GUID>0c130eb6-0414-4355-a04a-0f71860428de</GUID>
    <ObjectType>cms.webpartcategory</ObjectType>
  </WebPartCategoryID>
  <WebPartDescription>
    <![CDATA[Checks for spelling errors in search expressions and offers a link that searches for the modified search terms. It can be used together with any of the Smart or SQL search web parts.]]>
  </WebPartDescription>
  <WebPartDisplayName>Did you mean</WebPartDisplayName>
  <WebPartDocumentation />
  <WebPartFileName>Search\DidYouMean.ascx</WebPartFileName>
  <WebPartGUID>70d3f41d-b0da-4d5b-b538-725a866e5961</WebPartGUID>
  <WebPartIconClass>icon-w-did-you-mean</WebPartIconClass>
  <WebPartName>DidYouMean</WebPartName>
  <WebPartProperties>
    <form version="2">
      <field allowempty="true" column="Language" columnsize="10" columntype="text" guid="771661bd-8730-495c-bad3-18d24c8a9748" publicfield="false" resolvedefaultvalue="False" spellcheck="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Language</fieldcaption>
          <fielddescription>
            <![CDATA[Sets the culture code of the language that should be used to detect typing mistakes and recommend new words. If a code isn't specified, the culture of the current page is used.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="Text" columnsize="100" columntype="text" guid="bc18e957-1d1d-49e5-85b0-26fc4969369f" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <defaultvalue ismacro="true">{$search.didyoumean$}</defaultvalue>
          <fieldcaption>Text</fieldcaption>
          <fielddescription>
            <![CDATA[Sets the text displayed before the suggested words.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="StartTag" columnsize="50" columntype="text" guid="a72f9fb9-d45d-4827-8b8b-541a585a9c3b" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>
            <![CDATA[<strong>]]>
          </defaultvalue>
          <fieldcaption>Start tag</fieldcaption>
          <fielddescription>
            <![CDATA[HTML to be placed before the suggested words, typically used for formatting. The default value is <strong>.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="EndTag" columnsize="50" columntype="text" guid="e25ba5c1-f8cd-4d36-a255-4aa66bbcb6dd" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>
            <![CDATA[</strong>]]>
          </defaultvalue>
          <fieldcaption>End tag</fieldcaption>
          <fielddescription>
            <![CDATA[HTML to be placed after the suggested words, typically used to close formatting tags. The default value is </strong>.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
    </form>
  </WebPartProperties>
  <WebPartSkipInsertProperties>False</WebPartSkipInsertProperties>
  <WebPartType>0</WebPartType>
</cms.webpart>