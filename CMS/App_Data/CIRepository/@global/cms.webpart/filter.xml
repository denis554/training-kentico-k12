<?xml version="1.0" encoding="utf-8"?>
<cms.webpart>
  <WebPartCategoryID>
    <CodeName>Filters</CodeName>
    <GUID>b67bb1a5-4c13-4d58-98a3-7eafec237d74</GUID>
    <ObjectType>cms.webpartcategory</ObjectType>
  </WebPartCategoryID>
  <WebPartDescription>
    <![CDATA[This web part is used to place custom developed filters onto your website and connect them to a datasource. The path to the user control implementing the filter must be specified.]]>
  </WebPartDescription>
  <WebPartDisplayName>Filter</WebPartDisplayName>
  <WebPartDocumentation>
    <![CDATA[For more information and an example demonstrating how to develop custom filters, please see <a href="~/CMSPages/GetDocLink.ashx?link=custom_filters_developing" target="_blank">Developing custom filters</a>.]]>
  </WebPartDocumentation>
  <WebPartFileName>Filters/filter.ascx</WebPartFileName>
  <WebPartGUID>1652e67c-e71d-497d-a567-78d0f1cb312e</WebPartGUID>
  <WebPartIconClass>icon-w-filter</WebPartIconClass>
  <WebPartName>Filter</WebPartName>
  <WebPartProperties>
    <form version="2">
      <category name="Filter properties">
        <properties>
          <visible>True</visible>
        </properties>
      </category>
      <field column="FilterName" columnsize="200" columntype="text" guid="370dc324-7e04-4beb-b100-ffeace93b56a" publicfield="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Filter name</fieldcaption>
          <fielddescription>
            <![CDATA[Sets a name for the filter. It must match the name entered into the Filter name property of the data source or listing web part that should be filtered.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field column="FilterControlPath" columnsize="200" columntype="text" guid="984355b3-1bb2-4394-964e-0e5d8de00f68" publicfield="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Filter control path</fieldcaption>
          <fielddescription>
            <![CDATA[Specifies the virtual path to the .ascx file of the user control implementing the custom filter. For example: ~/CMSGlobalFiles/CustomFilter.ascx]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
    </form>
  </WebPartProperties>
  <WebPartSkipInsertProperties>False</WebPartSkipInsertProperties>
  <WebPartType>2</WebPartType>
</cms.webpart>