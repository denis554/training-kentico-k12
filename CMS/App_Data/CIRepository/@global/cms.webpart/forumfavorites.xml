<?xml version="1.0" encoding="utf-8"?>
<cms.webpart>
  <WebPartCategoryID>
    <CodeName>Forums</CodeName>
    <GUID>02855290-f69a-4f87-bb90-54b4a6ffd4b1</GUID>
    <ObjectType>cms.webpartcategory</ObjectType>
  </WebPartCategoryID>
  <WebPartDescription>
    <![CDATA[Displays a list of the favorite forum items of the current user. The items serve as links to the given forum section.]]>
  </WebPartDescription>
  <WebPartDisplayName>Forum favorites</WebPartDisplayName>
  <WebPartDocumentation />
  <WebPartFileName>Forums/ForumFavorites.ascx</WebPartFileName>
  <WebPartGUID>67a9f20e-7d8c-4a62-82dd-eac0331c5a4e</WebPartGUID>
  <WebPartIconClass>icon-w-forum-favorites</WebPartIconClass>
  <WebPartName>ForumFavorites</WebPartName>
  <WebPartProperties>
    <form version="2">
      <category name="Content">
        <properties>
          <visible>True</visible>
        </properties>
      </category>
      <field allowempty="true" column="SiteName" columnsize="100" columntype="text" guid="117e63f1-dbbf-4abd-bbef-440006d92e49" publicfield="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Site name</fieldcaption>
          <fielddescription>
            <![CDATA[Specifies the site from which forum favorites should be loaded. If empty, the current site will be used. It is necessary to correctly set the Forum URL property in order for the web part to generate links to favorite forum items from a different site than the current one.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>selectsite</controlname>
        </settings>
      </field>
      <field allowempty="true" column="ForumUrl" columnsize="200" columntype="text" guid="3034a47e-8ce5-4bd0-b4c6-9d22b06312af" publicfield="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Forum URL</fieldcaption>
          <fielddescription>
            <![CDATA[Specifies the base URL used in the links to the favorited forum items generated by the web part (e.g. /Forums.aspx). If left empty, the Base URL of the forum that contains the favorited item is used dynamically (if there is one defined). This is recommended, since it allows links to favorites from multiple forums in different locations.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="AllowEditing" columntype="boolean" guid="1bb6a9ca-e983-4bb4-aaef-ba9447a26293" publicfield="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>true</defaultvalue>
          <fieldcaption>Allow editing</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates whether users should be allowed to edit their list of forum favorites, i.e. delete items.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field allowempty="true" column="DeleteImageUrl" columnsize="200" columntype="text" guid="b0eb7a63-393d-414f-b534-9565a6b51d9a" publicfield="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Delete image URL</fieldcaption>
          <fielddescription>
            <![CDATA[Specifies the URL of the image used for the action icon that deletes forum favorites from the list. If empty, a default image will be used.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <category name="No data behaviour">
        <properties>
          <visible>True</visible>
        </properties>
      </category>
      <field allowempty="true" column="HideControlForZeroRows" columntype="boolean" guid="338cf444-b796-4968-8dda-519732e4c57e" publicfield="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>false</defaultvalue>
          <fieldcaption>Hide if no record found</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates if the web part should be hidden when there are no forum favorites to be displayed.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field allowempty="true" column="ZeroRowsText" columnsize="100" columntype="text" guid="3398415f-1224-4f10-8961-7ea6c01a8d5a" publicfield="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>No favorites found</defaultvalue>
          <fieldcaption>No record found text</fieldcaption>
          <fielddescription>
            <![CDATA[Text message that should be displayed if there are no forum favorites for the current user. This text will not be visible if the Hide if no record found property of the web part is enabled.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <category name="System settings">
        <properties>
          <visible>True</visible>
        </properties>
      </category>
      <field allowempty="true" column="CacheItemName" columnsize="200" columntype="text" guid="e38d6dd0-d513-4e62-8f54-e588d5e8f8c2" publicfield="false" visible="true">
        <properties>
          <fieldcaption>Cache item name</fieldcaption>
          <fielddescription>{$documentation.webpartproperties.cacheitemname$}</fielddescription>
        </properties>
        <settings>
          <controlname>textboxcontrol</controlname>
        </settings>
      </field>
      <field allowempty="true" column="CacheMinutes" columntype="integer" guid="e066f049-584b-4de3-8c51-b83f5774a2f5" publicfield="false" visible="true">
        <properties>
          <fieldcaption>Cache minutes</fieldcaption>
          <fielddescription>{$documentation.webpartproperties.cacheminutes$}</fielddescription>
        </properties>
        <settings>
          <controlname>textboxcontrol</controlname>
        </settings>
      </field>
      <field allowempty="true" column="CacheDependencies" columntype="longtext" guid="e538e91e-dbdd-4d10-aec8-51acfc3760d3" publicfield="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Cache dependencies</fieldcaption>
          <fielddescription>
            <![CDATA[{$documentation.webpartproperties.cachedependencies$}]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>cachedependencies</controlname>
        </settings>
      </field>
    </form>
  </WebPartProperties>
  <WebPartResourceID>
    <CodeName>CMS.Forums</CodeName>
    <GUID>16e96e6c-f16f-49dc-a640-2357418668b8</GUID>
    <ObjectType>cms.resource</ObjectType>
  </WebPartResourceID>
  <WebPartSkipInsertProperties>False</WebPartSkipInsertProperties>
  <WebPartType>0</WebPartType>
</cms.webpart>