          <rule>
            <![CDATA[{%Rule("Value <= 19", "<rules><r pos=\"0\" par=\"\" op=\"and\" n=\"MaxValue\" ><p n=\"maxvalue\"><t>19</t><v>19</v><r>false</r><d></d><vt>integer</vt></p></r></rules>")%}]]>
