<?xml version="1.0" encoding="utf-8"?>
<cms.webpart>
  <WebPartCategoryID>
    <CodeName>GroupMembers</CodeName>
    <GUID>fde9d969-0d1e-4720-83a4-f7093b413333</GUID>
    <ObjectType>cms.webpartcategory</ObjectType>
  </WebPartCategoryID>
  <WebPartDescription>
    <![CDATA[Manages invitations to join the group. The GUID of the invitation that should be managed must be passed to the web part through the URL via the 'invitationguid' query string parameter.]]>
  </WebPartDescription>
  <WebPartDisplayName>Group invitation</WebPartDisplayName>
  <WebPartDocumentation>
    <![CDATA[If you wish to allow users to easily accept or reject group invitations, it is recommended to use the <strong>My invitations</strong> web part instead.]]>
  </WebPartDocumentation>
  <WebPartFileName>Community/Membership/GroupInvitation.ascx</WebPartFileName>
  <WebPartGUID>f94a0a1b-f611-4e31-90be-155272ae9a7b</WebPartGUID>
  <WebPartIconClass>icon-w-group-invitation</WebPartIconClass>
  <WebPartName>GroupInvitation</WebPartName>
  <WebPartProperties>
    <form version="2">
      <category name="Group invitation properties">
        <properties>
          <visible>True</visible>
        </properties>
      </category>
      <field column="SendDefaultGroupEmails" columntype="boolean" guid="3dcd81ce-8d6a-447c-ab7d-0d8f5988502e" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>true</defaultvalue>
          <fieldcaption>Send default group e-mails</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates whether notification e-mails should be sent to the group's administrators about users who accept invitations. If true, the e-mails will be sent according to the configuration of the group's properties.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field column="SendEmailToInviter" columntype="boolean" guid="b996912f-4a8f-48ec-83cb-a33d1c2700b7" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>true</defaultvalue>
          <fieldcaption>Send e-mail to inviter</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates whether a confirmation e-mail should be sent to the user who created the invitation informing whether it was accepted.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field allowempty="true" column="UserIsAlreadyMember" columnsize="100" columntype="text" guid="5fde837b-d81a-4f36-98d6-46aa4f7728b8" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>User is already member of group caption</fieldcaption>
          <fielddescription>
            <![CDATA[Text displayed if the invited user is already a member of the group.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="InvitationIsNotValid" columnsize="100" columntype="text" guid="8fb22c51-5c5f-4771-9233-0c04cd0dcf79" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Invitation is not valid caption</fieldcaption>
          <fielddescription>
            <![CDATA[Text that is displayed if a user attempts to accept a group invitation that is no longer valid.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="GroupNoLongerExists" columnsize="100" columntype="text" guid="a6966b1c-0c30-4401-b65d-b01ed3a75856" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Group no longer exists caption</fieldcaption>
          <fielddescription>
            <![CDATA[Text that is displayed if a user attempts to accept an invitation to a group that no longer exists.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="MemberJoined" columnsize="100" columntype="text" guid="a3a29406-3a59-4cbf-98b8-82b28ff470df" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Member joined caption</fieldcaption>
          <fielddescription>
            <![CDATA[Text displayed when a user successfully accepts an invitation to join a group.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="MemberWaiting" columnsize="100" columntype="text" guid="e9418b57-3a4f-45ce-a96b-69ee10e524f8" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Member waiting caption</fieldcaption>
          <fielddescription>
            <![CDATA[Text displayed if a user successfully accepts an invitation, but requires further approval from the group's administrators to join.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="LoginURL" columnsize="100" columntype="text" guid="aa128c8c-5ef0-402c-a5a2-b308a99edfbb" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Logon page URL</fieldcaption>
          <fielddescription>
            <![CDATA[Specifies the URL of the logon page. If empty, then the value is loaded from the website settings.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
    </form>
  </WebPartProperties>
  <WebPartResourceID>
    <CodeName>CMS.Community</CodeName>
    <GUID>f1cc54a9-d5bb-4a69-bd8c-8918eb410656</GUID>
    <ObjectType>cms.resource</ObjectType>
  </WebPartResourceID>
  <WebPartSkipInsertProperties>False</WebPartSkipInsertProperties>
  <WebPartType>0</WebPartType>
</cms.webpart>