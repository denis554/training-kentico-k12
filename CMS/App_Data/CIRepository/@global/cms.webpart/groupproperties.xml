        <properties>
          <defaultvalue>You are not allowed to manage this group.</defaultvalue>
          <fieldcaption>No permissions message</fieldcaption>
          <fielddescription>{$webpart.properties.nopermissionmessage$}</fielddescription>
        </properties>
        <settings>
          <controlname>textboxcontrol</controlname>
        </settings>
      </field>
      <field allowempty="true" column="AllowChangeGroupDisplayName" columntype="boolean" guid="95fe90d9-2d29-464a-8d66-9ccde360c330" publicfield="false" spellcheck="false" visibility="none" visible="true">
