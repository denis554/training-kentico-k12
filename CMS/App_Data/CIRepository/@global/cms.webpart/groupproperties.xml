    <ObjectType>cms.webpartcategory</ObjectType>
  </WebPartCategoryID>
  <WebPartDescription>
    <![CDATA[Allows group administrators to edit the basic properties of the specified group directly from the live site.]]>
  </WebPartDescription>
  <WebPartDisplayName>Group properties</WebPartDisplayName>
  <WebPartDocumentation />
  <WebPartFileName>Community/Profile/GroupProperties.ascx</WebPartFileName>
  <WebPartGUID>a64e78e5-ad4b-4229-b873-a5cd62c5006b</WebPartGUID>
  <WebPartIconClass>icon-w-group-properties</WebPartIconClass>
  <WebPartName>GroupProperties</WebPartName>
  <WebPartProperties>
    <form version="2">
      <category name="Community">
        <properties>
          <visible>True</visible>
        </properties>
      </category>
      <field column="GroupName" columnsize="100" columntype="text" guid="7fc1af8c-538a-4470-be35-b16b7d935645" publicfield="false" visible="true">
        <properties>
          <fieldcaption>Group name</fieldcaption>
          <fielddescription>{$documentation.webpartproperties.groupname$}</fielddescription>
        </properties>
        <settings>
          <controlname>communitygroupselector</controlname>
        </settings>
      </field>
