          <defaultvalue>false</defaultvalue>
          <fieldcaption>Allow change group  display name</fieldcaption>
          <fielddescription>
            <![CDATA[{$documentation.webpart.allowuserchangedisplayname$}]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field allowempty="true" column="AllowSelectTheme" columntype="boolean" guid="bcbd3b4b-95e7-4ebf-a705-811f9d735a75" publicfield="false" spellcheck="false" visibility="none" visible="true">
