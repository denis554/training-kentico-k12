          <defaultvalue>false</defaultvalue>
          <fieldcaption>Allow select theme</fieldcaption>
          <fielddescription>{$documentation.allowselecttheme$}</fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
    </form>
  </WebPartProperties>
  <WebPartResourceID>
    <CodeName>CMS.Community</CodeName>
    <GUID>f1cc54a9-d5bb-4a69-bd8c-8918eb410656</GUID>
