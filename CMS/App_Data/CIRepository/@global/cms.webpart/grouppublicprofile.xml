<?xml version="1.0" encoding="utf-8"?>
<cms.webpart>
  <WebPartCategoryID>
    <CodeName>CommunityGroups</CodeName>
    <GUID>c745ceae-0786-4260-85a5-4558f82c027d</GUID>
    <ObjectType>cms.webpartcategory</ObjectType>
  </WebPartCategoryID>
  <WebPartDescription>
    <![CDATA[Displays the public profile of the specified group. The layout and content of the profile is defined by a selected form.]]>
  </WebPartDescription>
  <WebPartDisplayName>Group public profile</WebPartDisplayName>
  <WebPartDocumentation />
  <WebPartFileName>Community/Groups/GroupPublicProfile.ascx</WebPartFileName>
  <WebPartGUID>72c52a91-cbde-4b98-86a2-e48d56a64391</WebPartGUID>
  <WebPartIconClass>icon-w-group-public-profile</WebPartIconClass>
  <WebPartName>GroupPublicProfile</WebPartName>
  <WebPartProperties>
    <form version="2">
      <category name="Community">
        <properties>
          <visible>True</visible>
        </properties>
      </category>
      <field column="GroupName" columnsize="100" columntype="text" guid="6297e113-ddd8-4e6b-af01-70a3ab95baa2" publicfield="false" visible="true">
        <properties>
          <fieldcaption>Group  name</fieldcaption>
          <fielddescription>{$documentation.webpartproperties.groupname$}</fielddescription>
        </properties>
        <settings>
          <controlname>communitygroupselector</controlname>
        </settings>
      </field>
      <category name="Public profile settings">
        <properties>
          <visible>True</visible>
        </properties>
      </category>
      <field column="AlternativeFormName" columnsize="200" columntype="text" guid="fc021d08-d1e0-4095-9103-6b1e29034a20" publicfield="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>Community.Group.DisplayProfile</defaultvalue>
          <fieldcaption>Alternative form name</fieldcaption>
          <fielddescription>
            <![CDATA[{$documentation.webpartproperties.alternativeformname$}]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>selectalternativeform</controlname>
        </settings>
      </field>
      <field column="NoProfileText" columnsize="250" columntype="text" guid="a5b350ee-8bca-46e4-ae63-ba92bb9c8354" publicfield="false" visible="true">
        <properties>
          <defaultvalue>Requested profile not found.</defaultvalue>
          <fieldcaption>No profile text</fieldcaption>
          <fielddescription>{$documentation.webpartproperties.noprofiletext$}</fielddescription>
        </properties>
        <settings>
          <cols>33</cols>
          <controlname>textareacontrol</controlname>
          <rows>5</rows>
        </settings>
      </field>
    </form>
  </WebPartProperties>
  <WebPartResourceID>
    <CodeName>CMS.Community</CodeName>
    <GUID>f1cc54a9-d5bb-4a69-bd8c-8918eb410656</GUID>
    <ObjectType>cms.resource</ObjectType>
  </WebPartResourceID>
  <WebPartSkipInsertProperties>False</WebPartSkipInsertProperties>
  <WebPartType>0</WebPartType>
</cms.webpart>