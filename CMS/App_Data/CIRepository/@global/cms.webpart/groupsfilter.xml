<?xml version="1.0" encoding="utf-8"?>
<cms.webpart>
  <WebPartCategoryID>
    <CodeName>CommunityGroups</CodeName>
    <GUID>c745ceae-0786-4260-85a5-4558f82c027d</GUID>
    <ObjectType>cms.webpartcategory</ObjectType>
  </WebPartCategoryID>
  <WebPartDescription>
    <![CDATA[Displays a filter that can be used to limit which groups should be displayed from the connected data source. It is intended for use in combination with a Groups data source web part.]]>
  </WebPartDescription>
  <WebPartDisplayName>Groups filter</WebPartDisplayName>
  <WebPartDocumentation />
  <WebPartFileName>Community/Groups/GroupsFilter.ascx</WebPartFileName>
  <WebPartGUID>8f15e36d-9d37-48fe-bd44-65832c5af23c</WebPartGUID>
  <WebPartIconClass>icon-w-groups-filter</WebPartIconClass>
  <WebPartName>GroupsFilter</WebPartName>
  <WebPartProperties>
    <form version="2">
      <category name="Filter properties">
        <properties>
          <visible>True</visible>
        </properties>
      </category>
      <field allowempty="true" column="SortGroupNameLinkText" columnsize="200" columntype="text" guid="0f5a16db-3715-425f-ae34-cd4842801bd2" publicfield="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Group name sort link text</fieldcaption>
          <fielddescription>
            <![CDATA[Sets the text caption of the filter's "sort by name" link. This link causes the filter to sort the displayed groups according to their display name.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="SortGroupCreatedLinkText" columnsize="200" columntype="text" guid="6d6a4970-e407-4135-9a51-8552de2f73db" publicfield="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Group created sort link text</fieldcaption>
          <fielddescription>
            <![CDATA[Sets the text caption of the filter's "sort by creation date" link. This link causes the filter to sort the displayed groups according to the date when they were created.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="ButtonText" columnsize="200" columntype="text" guid="3c6ceece-92f0-42d5-b2ec-314d3ed6595d" publicfield="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Button text</fieldcaption>
          <fielddescription>
            <![CDATA[Sets the text displayed as the caption of the button that applies the filter to the data.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="DisableFilterCaching" columntype="boolean" guid="66a84319-ae28-4550-9286-3a49d570b02b" publicfield="false" visible="true">
        <properties>
          <defaultvalue>false</defaultvalue>
          <fieldcaption>Disables filter caching</fieldcaption>
          <fielddescription>
            <![CDATA[{$documentation.webpartproperties.disablefiltercaching$}]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
    </form>
  </WebPartProperties>
  <WebPartResourceID>
    <CodeName>CMS.Community</CodeName>
    <GUID>f1cc54a9-d5bb-4a69-bd8c-8918eb410656</GUID>
    <ObjectType>cms.resource</ObjectType>
  </WebPartResourceID>
  <WebPartSkipInsertProperties>False</WebPartSkipInsertProperties>
  <WebPartType>2</WebPartType>
</cms.webpart>