<?xml version="1.0" encoding="utf-8"?>
<cms.webpart>
  <WebPartCategoryID>
    <CodeName>Messaging</CodeName>
    <GUID>e04cf39b-67c5-4527-b48a-64308124c549</GUID>
    <ObjectType>cms.webpartcategory</ObjectType>
  </WebPartCategoryID>
  <WebPartDescription>
    <![CDATA[The Inbox web part displays recieved messages for current user.]]>
  </WebPartDescription>
  <WebPartDisplayName>Inbox</WebPartDisplayName>
  <WebPartDocumentation />
  <WebPartFileName>messaging/inbox.ascx</WebPartFileName>
  <WebPartGUID>894707f5-30a9-4f78-a88a-3f8e6cf8261d</WebPartGUID>
  <WebPartIconClass>icon-w-inbox</WebPartIconClass>
  <WebPartName>inbox</WebPartName>
  <WebPartProperties>
    <form version="2">
      <category name="Design">
        <properties>
          <visible>True</visible>
        </properties>
      </category>
      <field allowempty="true" column="PageSize" columntype="integer" guid="c3da9f01-d562-4bdd-9165-c49a29ae5e1b" publicfield="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>10</defaultvalue>
          <fieldcaption>Page size</fieldcaption>
          <fielddescription>
            <![CDATA[Sets the maximum amount of messages displayed per page.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="ZeroRowsText" columnsize="200" columntype="text" guid="fdca0ac9-020f-45f9-9244-e01ce36f09fe" publicfield="false" visible="true">
        <properties>
          <defaultvalue>There are currently no messages in the inbox.</defaultvalue>
          <fieldcaption>No record found text</fieldcaption>
          <fielddescription>{$documentation.webpartproperties.norecordtext$}</fielddescription>
        </properties>
        <settings>
          <controlname>textboxcontrol</controlname>
        </settings>
      </field>
      <field allowempty="true" column="PasteOriginalMessage" columntype="boolean" guid="7e5a6fa7-90f9-411d-bd5b-05053f12a896" publicfield="false" visible="true">
        <properties>
          <defaultvalue>true</defaultvalue>
          <fieldcaption>Paste original message</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates if original message text should be included in the response text.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field allowempty="true" column="ShowOriginalMessage" columntype="boolean" guid="f65471d7-c4c7-426b-be20-572c647738ac" publicfield="false" visible="true">
        <properties>
          <defaultvalue>true</defaultvalue>
          <fieldcaption>Show original message</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates if original message text shoud be displayed when replying.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
    </form>
  </WebPartProperties>
  <WebPartResourceID>
    <CodeName>CMS.Messaging</CodeName>
    <GUID>2b521e04-6b2d-4422-b1d2-c352e88e3b02</GUID>
    <ObjectType>cms.resource</ObjectType>
  </WebPartResourceID>
  <WebPartSkipInsertProperties>False</WebPartSkipInsertProperties>
  <WebPartType>0</WebPartType>
</cms.webpart>