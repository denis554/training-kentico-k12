<?xml version="1.0" encoding="utf-8"?>
<cms.webpart>
  <WebPartCategoryID>
    <CodeName>MembershipLogonRegistration</CodeName>
    <GUID>d01e7e0e-fa1e-4a29-ac4b-940bfcba4c1e</GUID>
    <ObjectType>cms.webpartcategory</ObjectType>
  </WebPartCategoryID>
  <WebPartDescription>
    <![CDATA[This web part generates hidden requests for the given page to prevent the current user's session from expiring. The time interval between requests can be specified.]]>
  </WebPartDescription>
  <WebPartDisplayName>Keep alive</WebPartDisplayName>
  <WebPartDocumentation>
    <![CDATA[If&nbsp;the web part is&nbsp;disabled due to its property settings, the session expires in the usual way.]]>
  </WebPartDocumentation>
  <WebPartFileName>Membership\KeepAlive.ascx</WebPartFileName>
  <WebPartGUID>84b0027f-0260-43f8-b6ff-ec7294b224fb</WebPartGUID>
  <WebPartIconClass>icon-w-keep-alive</WebPartIconClass>
  <WebPartName>KeepAlive</WebPartName>
  <WebPartProperties>
    <form version="2">
      <category name="Timer settings">
        <properties>
          <visible>True</visible>
        </properties>
      </category>
      <field column="RefreshingInterval" columntype="integer" guid="ac336e7a-776e-4a35-9f0e-925c43f3bd9e" publicfield="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>5</defaultvalue>
          <fieldcaption>Refreshing interval</fieldcaption>
          <fielddescription>Sets the interval between requests in seconds.</fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
    </form>
  </WebPartProperties>
  <WebPartSkipInsertProperties>False</WebPartSkipInsertProperties>
  <WebPartType>4</WebPartType>
</cms.webpart>