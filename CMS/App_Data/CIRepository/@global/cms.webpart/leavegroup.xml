<?xml version="1.0" encoding="utf-8"?>
<cms.webpart>
  <WebPartCategoryID>
    <CodeName>GroupMembers</CodeName>
    <GUID>fde9d969-0d1e-4720-83a4-f7093b413333</GUID>
    <ObjectType>cms.webpartcategory</ObjectType>
  </WebPartCategoryID>
  <WebPartDescription>
    <![CDATA[Allows to users to leave the currently active group (cancel their membership). The current group is taken from the context, which means that the web part must be placed onto a page that is owned by a group in order to work correctly.]]>
  </WebPartDescription>
  <WebPartDisplayName>Leave group</WebPartDisplayName>
  <WebPartDocumentation />
  <WebPartFileName>Community/Membership/LeaveGroup.ascx</WebPartFileName>
  <WebPartGUID>57a39d42-f60a-4d8f-b307-1d48223b3f50</WebPartGUID>
  <WebPartIconClass>icon-w-leave-group</WebPartIconClass>
  <WebPartName>LeaveGroup</WebPartName>
  <WebPartProperties>
    <form version="2">
      <category name="General properties">
        <properties>
          <visible>True</visible>
        </properties>
      </category>
      <field column="LeaveText" columnsize="250" columntype="text" guid="3b117a54-2bf1-4bcf-ba8d-d0002e482ce7" publicfield="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>Are you sure?</defaultvalue>
          <fieldcaption>Leave text</fieldcaption>
          <fielddescription>
            <![CDATA[Sets the text displayed when users attempt to leave the group. This can be used to specify a confirmation question.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="SuccessfullLeaveText" columnsize="250" columntype="text" guid="07939c78-ebc8-4e54-8d72-476b4067f497" publicfield="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Successfull leave text</fieldcaption>
          <fielddescription>
            <![CDATA[Text displayed after a user successfully leaves the group.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="UnSuccessfullLeaveText" columnsize="250" columntype="text" guid="b33945bf-a780-4c5d-bb8f-d3a06b308366" publicfield="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Unsuccessfull leave text</fieldcaption>
          <fielddescription>
            <![CDATA[Text displayed if the user cannot be removed from the the group (e.g. if they are no longer a member).]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
    </form>
  </WebPartProperties>
  <WebPartSkipInsertProperties>False</WebPartSkipInsertProperties>
  <WebPartType>0</WebPartType>
</cms.webpart>