          <defaultvalue>false</defaultvalue>
          <fieldcaption>Display error as popup</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates if the authentication failure text should be displayed in a popup window.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field column="PersistentLogin" columntype="boolean" guid="7d697b81-b0ce-47bf-988a-46fee11c82eb" publicfield="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>false</defaultvalue>
          <fieldcaption>Persistent login</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates whether a cookie should be used to remember users that logged in successfully. This will allow them to be authenticated automatically when they return.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
