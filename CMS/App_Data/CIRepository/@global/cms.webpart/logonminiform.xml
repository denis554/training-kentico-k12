      <category name="Logon form settings">
        <properties>
          <visible>True</visible>
        </properties>
      </category>
      <field allowempty="true" column="DefaultTargetURL" columnsize="500" columntype="text" guid="1e812344-d31f-4392-9fb8-fdcbef807842" publicfield="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Default target URL</fieldcaption>
          <fielddescription>
