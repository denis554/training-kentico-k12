          <defaultvalue>false</defaultvalue>
          <fieldcaption>Show image button</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates if the login button should be displayed as an image.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
