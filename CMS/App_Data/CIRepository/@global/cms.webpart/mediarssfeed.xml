<?xml version="1.0" encoding="utf-8"?>
<cms.webpart>
  <WebPartCategoryID>
    <CodeName>SyndicationRSS</CodeName>
    <GUID>fcec1ff1-21ce-48d3-8a63-3d96de0c5ccf</GUID>
    <ObjectType>cms.webpartcategory</ObjectType>
  </WebPartCategoryID>
  <WebPartDescription>
    <![CDATA[This web part creates an RSS feed of files stored in Kentico media libraries. Uses a built-in media file data source as a source of the feed data. Displays a link leading to the feed, which is located at the URL of the page where the web part is placed, with a querystring parameter appended.]]>
  </WebPartDescription>
  <WebPartDisplayName>Media files RSS feed</WebPartDisplayName>
  <WebPartFileName>MediaLibrary/Syndication/MediaRSSFeed.ascx</WebPartFileName>
  <WebPartGUID>455e445c-6cee-4ab9-a8a9-90bdc8dbb53e</WebPartGUID>
  <WebPartIconClass>icon-w-media-files-rss-feed</WebPartIconClass>
  <WebPartName>MediaRSSFeed</WebPartName>
