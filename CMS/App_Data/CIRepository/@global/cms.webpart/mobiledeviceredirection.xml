        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textareacontrol</controlname>
          <FilterMode>False</FilterMode>
          <IsTextArea>True</IsTextArea>
        </settings>
      </field>
      <field allowempty="true" column="PreserveQueryString" columntype="boolean" guid="c48a31c5-cef7-4ed0-b47f-1f6f4110d1f9" publicfield="false" visible="true">
        <properties>
          <defaultvalue>false</defaultvalue>
          <fieldcaption>Preserve query string</fieldcaption>
          <fielddescription>
            <![CDATA[If enabled, the web part transfers all query string parameters from the original page URL to the mobile redirection URLs.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
    </form>
  </WebPartProperties>
  <WebPartSkipInsertProperties>False</WebPartSkipInsertProperties>
  <WebPartType>4</WebPartType>
</cms.webpart>