          <fielddescription>
            <![CDATA[Indicates if the My pending requests tab should be displayed.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field column="DisplayFriendsRejected" columntype="boolean" guid="06a3e7a3-b9c2-4276-a979-a4bfde268295" publicfield="false" visibility="none" visible="true">
