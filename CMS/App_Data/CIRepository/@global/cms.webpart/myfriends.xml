          <defaultvalue>true</defaultvalue>
          <fieldcaption>Display rejected friendships</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates if the Rejected friends tab should be displayed.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
