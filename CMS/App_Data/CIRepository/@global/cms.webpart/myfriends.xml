            <![CDATA[Indicates if the Friends waiting for my approval tab should be displayed.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field column="DisplayFriendsRequested" columntype="boolean" guid="ad5a452e-f9f1-4506-a722-2440c824dfd9" publicfield="false" visibility="none" visible="true">
