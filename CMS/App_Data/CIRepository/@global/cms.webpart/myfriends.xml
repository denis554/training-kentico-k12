          <fielddescription>
            <![CDATA[Indicates if the My friends tab should be displayed.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field column="DisplayFriendsToApproval" columntype="boolean" guid="e407d272-6558-4df7-b779-47e732bfb74a" publicfield="false" visibility="none" visible="true">
