<?xml version="1.0" encoding="utf-8"?>
<cms.webpart>
  <WebPartCategoryID>
    <CodeName>Newsletters</CodeName>
    <GUID>fc9fe123-adb8-4a87-9c79-1f96afb20587</GUID>
    <ObjectType>cms.webpartcategory</ObjectType>
  </WebPartCategoryID>
  <WebPartDescription>
    <![CDATA[Displays a dialog that allows visitors to subscribe to website's newsletters.]]>
  </WebPartDescription>
  <WebPartDisplayName>Newsletter subscription</WebPartDisplayName>
  <WebPartDocumentation>
<![CDATA[
<p>
 <span style="color: rgb(0,0,0)">The design of the web part can be&nbsp;modified using standard CSS styles, a custom skin, or by&nbsp;editing the <b>CMSWebParts\Newsletters\NewsletterSubscriptionWebPart.ascx</b> file.</span></p>

]]>
  </WebPartDocumentation>
  <WebPartFileName>Newsletters/NewsletterSubscriptionWebPart.ascx</WebPartFileName>
  <WebPartGUID>355520d0-d42f-4445-819d-8cf426a3c6ce</WebPartGUID>
  <WebPartIconClass>icon-w-newsletter-subscription</WebPartIconClass>
  <WebPartName>NewsletterSubscription</WebPartName>
  <WebPartProperties>
    <form version="2">
      <category name="Dialog settings">
        <properties>
          <visible>True</visible>
        </properties>
      </category>
      <field column="NewsletterName" columnsize="400" columntype="text" guid="920cf472-b0b0-4b2d-888d-9af0feb40d62" publicfield="false" reftype="Required" resolvedefaultvalue="False" visibility="none" visible="true">
        <properties>
          <fieldcaption>{$emailmarketing.selector.newslettername.label$}</fieldcaption>
          <fielddescription>
            <![CDATA[Selects the newsletter to which users will be able to subscribe through the dialog. The (let the user choose) option allows users to select from all newsletters on the current site using checkboxes displayed below the dialog.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>newsletterselector</controlname>
          <EmailFeedSelection>newsletters</EmailFeedSelection>
        </settings>
      </field>
      <field allowempty="true" column="DisplayFirstName" columntype="boolean" guid="1301c149-7a17-4e70-b5d8-0d77efc2295f" publicfield="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>true</defaultvalue>
          <fieldcaption>Display first name</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates if the First name field should be included in the dialog.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field allowempty="true" column="FirstNameText" columnsize="200" columntype="text" guid="224aa388-070a-4a53-abb4-00cdc822817f" publicfield="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>First name label text</fieldcaption>
          <fielddescription>
            <![CDATA[Sets the text displayed before the First name field.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
