        <properties>
          <defaultvalue>true</defaultvalue>
          <fieldcaption>Display last name</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates if the Last name field should be included in the dialog.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field allowempty="true" column="LastNameText" columnsize="200" columntype="text" guid="873a46dc-96e4-444d-aef1-532556c540d7" publicfield="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Last name label text</fieldcaption>
          <fielddescription>
            <![CDATA[Sets the text displayed before the Last name field.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="EmailText" columnsize="200" columntype="text" guid="0415bff5-2b3f-4d73-ba79-f1bbbc1aeb7a" publicfield="false" resolvedefaultvalue="False" visibility="none" visible="true">
        <properties>
          <fieldcaption>Email label text</fieldcaption>
          <fielddescription>Sets the text displayed before the Email field.</fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>TextBoxControl</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
