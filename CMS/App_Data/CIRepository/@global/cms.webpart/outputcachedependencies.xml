<?xml version="1.0" encoding="utf-8"?>
<cms.webpart>
  <WebPartCategoryID>
    <CodeName>GeneralActions</CodeName>
    <GUID>00086044-1f49-4574-ab89-9cd537147b34</GUID>
    <ObjectType>cms.webpartcategory</ObjectType>
  </WebPartCategoryID>
  <WebPartDescription>
    <![CDATA[Defines cache dependencies for the output cache of the page where the web part is placed.]]>
  </WebPartDescription>
  <WebPartDisplayName>Output cache dependencies</WebPartDisplayName>
  <WebPartDocumentation>
<![CDATA[
You can configure the output cache settings of individual pages in the website content tree in <strong>Pages -&gt; Edit -&gt; Properties -&gt; General</strong>.<br />
<br />
For more information, see:
<ul>
	<li><a href="~/CMSPages/GetDocLink.ashx?link=cache_dependencies" target="_blank">Setting cache dependencies</a></li>
	<li><a href="~/CMSPages/GetDocLink.ashx?link=caching_output" target="_blank">Caching page output</a></li>
	<li><a href="~/CMSPages/GetDocLink.ashx?link=caching_config" target="_blank">Configuring caching</a></li>
</ul>

]]>
  </WebPartDocumentation>
  <WebPartFileName>General/OutputCacheDependencies.ascx</WebPartFileName>
  <WebPartGUID>0353a625-a95b-4202-9181-22423f924ea1</WebPartGUID>
  <WebPartIconClass>icon-w-output-cache-dependencies</WebPartIconClass>
  <WebPartName>OutputCacheDependencies</WebPartName>
  <WebPartProperties>
    <form version="2">
      <category name="Dependencies">
        <properties>
          <visible>True</visible>
        </properties>
      </category>
      <field column="UseDefaultDependencies" columntype="boolean" guid="c5821ef1-c396-4eee-bfa3-0de487c1d148" publicfield="false" resolvedefaultvalue="False" spellcheck="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>True</defaultvalue>
          <fieldcaption>Use default dependencies</fieldcaption>
          <fielddescription>
            <![CDATA[If checked, the page's output cache uses default dependencies in addition to the manually added dependencies. The default dependencies clear the cache whenever a user modifies the current page's content, properties or page template. If no custom dependencies are defined, the default dependencies are used automatically.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>CheckBoxControl</controlname>
        </settings>
      </field>
      <field column="CacheDependencies" columntype="longtext" guid="136deaf2-e888-4853-85c8-e1e6a99ed23d" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Cache dependencies</fieldcaption>
          <fielddescription>
            <![CDATA[Specifies a list of dummy cache keys on which the page's output cache depends. When the objects represented by the dummy keys are modified, the system clears the page's output cache. Add one dummy key per line. ]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <Cols>5</Cols>
          <controlname>textareacontrol</controlname>
          <FilterMode>False</FilterMode>
          <Rows>10</Rows>
          <Wrap>True</Wrap>
        </settings>
      </field>
    </form>
  </WebPartProperties>
  <WebPartSkipInsertProperties>False</WebPartSkipInsertProperties>
  <WebPartType>4</WebPartType>
</cms.webpart>