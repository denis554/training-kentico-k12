<?xml version="1.0" encoding="utf-8"?>
<cms.webpart>
  <WebPartCategoryID>
    <CodeName>ListingsAndViewers</CodeName>
    <GUID>25ea0b9d-7959-4566-b760-adddfbb8c159</GUID>
    <ObjectType>cms.webpartcategory</ObjectType>
  </WebPartCategoryID>
  <WebPartDescription>
    <![CDATA[Displays the number of page views that the page it is placed on has.]]>
  </WebPartDescription>
  <WebPartDisplayName>Page views</WebPartDisplayName>
  <WebPartDocumentation />
  <WebPartFileName>Viewers/PageViews.ascx</WebPartFileName>
  <WebPartGUID>94c64037-a56d-4b6f-a4f9-f0e5ce41b517</WebPartGUID>
  <WebPartIconClass>icon-w-page-views</WebPartIconClass>
  <WebPartName>PageViews</WebPartName>
  <WebPartProperties>
    <form version="2">
      <category name="Misc">
        <properties>
          <visible>True</visible>
        </properties>
      </category>
      <field allowempty="true" column="MessageText" columnsize="200" columntype="text" guid="855e45f2-7ab8-40d0-9b31-728d05baf742" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>Page views: {0}</defaultvalue>
          <fieldcaption>Message text</fieldcaption>
          <fielddescription>
            <![CDATA[Specifies the text displayed by the web part. The {0} macro expression can be used to display the amount of page views.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>textboxcontrol</controlname>
          <FilterEnabled>False</FilterEnabled>
          <FilterMode>False</FilterMode>
        </settings>
      </field>
      <field column="StatisticsType" columntype="integer" guid="94870c67-2ac5-494f-a718-323a61b54c30" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>0</defaultvalue>
          <fieldcaption>Statistics type</fieldcaption>
          <fielddescription>
            <![CDATA[Only page views that happened during the time interval selected in this property will be counted into the displayed amount.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>dropdownlistcontrol</controlname>
          <Options>
            <![CDATA[<item value="0" text="Current day" /><item value="1" text="Current week" /><item value="2" text="Current month" /><item value="3" text="Current year" /><item value="4" text="Total" />]]>
          </Options>
        </settings>
      </field>
    </form>
  </WebPartProperties>
  <WebPartSkipInsertProperties>False</WebPartSkipInsertProperties>
  <WebPartType>0</WebPartType>
</cms.webpart>