      <category name="Redirection settings">
        <properties>
          <visible>True</visible>
        </properties>
      </category>
      <field allowempty="true" column="RedirectionURLs" columntype="longtext" guid="7e5aa7e9-c99a-4ac1-b7ab-282047a7ab45" publicfield="false" spellcheck="false" visibility="none" visible="true">
