<?xml version="1.0" encoding="utf-8"?>
<cms.webpart>
  <WebPartCategoryID>
    <CodeName>Ecommerce</CodeName>
    <GUID>afabab19-af7d-49f8-ae0e-340ebe3ea3a1</GUID>
    <ObjectType>cms.webpartcategory</ObjectType>
  </WebPartCategoryID>
  <WebPartDefaultValues>
    <form />
  </WebPartDefaultValues>
  <WebPartDescription>
    <![CDATA[Checkout process wizard. Allows users to purchase items from their shopping cart. This web part incorporates all steps of a usual checkout process. If you want to customize the checkout process, use individual checkout process web parts instead of this one.]]>
  </WebPartDescription>
  <WebPartDisplayName>Checkout process (obsolete)</WebPartDisplayName>
  <WebPartFileName>Ecommerce/ShoppingCart/ShoppingCartWebPart.ascx</WebPartFileName>
  <WebPartGUID>8fa92a44-25cb-4e37-af4a-95f8482f3937</WebPartGUID>
  <WebPartIconClass>icon-w-checkout-process-obsolete</WebPartIconClass>
  <WebPartName>ShoppingCart</WebPartName>
  <WebPartProperties>
    <form version="2">
      <category name="General">
        <properties>
          <visible>True</visible>
        </properties>
      </category>
      <field allowempty="true" column="RedirectAfterPurchase" columnsize="500" columntype="text" guid="f26d863e-8db5-4c22-8896-2062a3bad812" publicfield="false" visible="true">
        <properties>
          <fieldcaption>Default URL after purchase</fieldcaption>
          <fielddescription>
            <![CDATA[Default page where the user should be redirected if no URL is specified for the given payment option.]]>
          </fielddescription>
        </properties>
        <settings>
          <AllowSetPermissions>False</AllowSetPermissions>
          <controlname>selectpath</controlname>
          <SinglePathMode>False</SinglePathMode>
        </settings>
      </field>
      <field column="PasswordRetrieval" columntype="boolean" guid="1da0d325-2442-4387-9749-4d5c2d9cf2c1" publicfield="false" visible="true">
        <properties>
          <defaultvalue>True</defaultvalue>
          <fieldcaption>Allow forgotten password retrieval</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates if the forgotten password can be retrieved in the Shopping Cart sign-in dialog.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field allowempty="true" column="DisplayStepImages" columntype="boolean" guid="eda3cf11-ba7e-4174-b59c-1ef16536957b" publicfield="false" visible="true">
        <properties>
          <defaultvalue>false</defaultvalue>
          <fieldcaption>Display step images</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates if the images should be displayed during the order process]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field allowempty="true" column="ImageStepSeparator" columnsize="500" columntype="text" guid="32624bdc-ef7f-457e-9c5e-c8505c99e623" publicfield="false" visible="true">
        <properties>
          <fieldcaption>Image step separator</fieldcaption>
          <fielddescription>
            <![CDATA[The separator displayed between shopping progress images.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>largetextarea</controlname>
        </settings>
      </field>
      <field allowempty="true" column="EnableProductPriceDetail" columntype="boolean" guid="0704a2ae-3e43-4eed-9f0c-0bf8abb8f9ad" publicfield="false" visible="true">
        <properties>
          <defaultvalue>false</defaultvalue>
          <fieldcaption>Enable product price detail</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates if the link to the price detail page should be displayed.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field allowempty="true" column="RequiredFieldsMark" columnsize="200" columntype="text" guid="ff028f3c-7ec3-4104-bc0d-19b795b2c569" publicfield="false" visible="true">
        <properties>
          <fieldcaption>Required fields mark</fieldcaption>
          <fielddescription>
            <![CDATA[HTML code for the required fields mark, e.g. asterisk (*)]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>largetextarea</controlname>
        </settings>
      </field>
      <category name="RegistrationForm">
        <properties>
          <caption>Registration form</caption>
          <visible>True</visible>
        </properties>
      </category>
      <field allowempty="true" column="AssignToRoles" columnsize="500" columntype="text" guid="6edbec86-4ecc-45af-8cb7-d67257f93e8e" publicfield="false" visible="true">
        <properties>
          <fieldcaption>Assign user to roles</fieldcaption>
          <fielddescription>
            <![CDATA[If you enter some role to this field, the user will be automatically assigned to it after registration.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>rolecheckboxselector</controlname>
        </settings>
      </field>
      <field allowempty="true" column="AssignToSites" columnsize="500" columntype="text" guid="8738d23d-bcb9-4f0d-9a50-5cffc9f0a939" publicfield="false" visible="true">
        <properties>
          <fieldcaption>Assign to sites</fieldcaption>
          <fielddescription>
            <![CDATA[If you enter some site here, users will be automatically assigned to the site after registration.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>simplecheckboxsiteselector</controlname>
        </settings>
      </field>
      <field allowempty="true" column="SendNewRegistrationNotificationToAddress" columnsize="254" columntype="text" guid="c8a8968b-6644-41d2-8c41-c5e18a3f9e7f" publicfield="false" visible="true">
        <properties>
          <fieldcaption>
            <![CDATA[Notify administrator about new registrations to e-mail address]]>
          </fieldcaption>
          <fielddescription>
            <![CDATA[Enter administrator's e-mail address if you want to send registration notification message.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>emailinput</controlname>
        </settings>
      </field>
      <category name="ConversionTracking">
        <properties>
          <caption>Conversion tracking</caption>
          <visible>True</visible>
        </properties>
      </category>
      <field allowempty="true" column="RegistrationTrackConversionName" columnsize="400" columntype="text" guid="c45d5f3f-cf4b-4dac-8957-bd2d8520b35f" publicfield="false" visible="true">
        <properties>
          <fieldcaption>Registration conversion name</fieldcaption>
          <fielddescription>
            <![CDATA[Selects the conversion that will be logged when a user successfully registers on the website through the web part.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>conversionselector</controlname>
        </settings>
      </field>
      <field allowempty="true" column="OrderTrackConversionName" columnsize="400" columntype="text" guid="563df6f3-47e6-4203-9bb6-83f33b442611" publicfield="false" visible="true">
        <properties>
          <fieldcaption>Order conversion name</fieldcaption>
          <fielddescription>
            <![CDATA[Selects the conversion that will be logged when a user completes a product order.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>conversionselector</controlname>
        </settings>
      </field>
    </form>
  </WebPartProperties>
  <WebPartResourceID>
    <CodeName>CMS.Ecommerce</CodeName>
    <GUID>684e021b-8b56-4cbf-8fd5-b7a791fd2dbc</GUID>
    <ObjectType>cms.resource</ObjectType>
  </WebPartResourceID>
  <WebPartSkipInsertProperties>False</WebPartSkipInsertProperties>
  <WebPartType>0</WebPartType>
</cms.webpart>