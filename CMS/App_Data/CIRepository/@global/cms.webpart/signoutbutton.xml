<?xml version="1.0" encoding="utf-8"?>
<cms.webpart>
  <WebPartCategoryID>
    <CodeName>MembershipLogonRegistration</CodeName>
    <GUID>d01e7e0e-fa1e-4a29-ac4b-940bfcba4c1e</GUID>
    <ObjectType>cms.webpartcategory</ObjectType>
  </WebPartCategoryID>
  <WebPartDescription>
    <![CDATA[Displays a button or link that allows users to log out. Once signed out, users will be redirected to a specified URL. The web part can also be used to provide a sign in button for unauthenticated users.]]>
  </WebPartDescription>
  <WebPartDisplayName>Sign out button</WebPartDisplayName>
  <WebPartDocumentation />
  <WebPartFileName>Membership/Logon/signoutbutton.ascx</WebPartFileName>
  <WebPartGUID>161ac7bc-ce69-4426-9913-0a53dc0366b3</WebPartGUID>
  <WebPartIconClass>icon-w-sign-out-button</WebPartIconClass>
  <WebPartName>SignOutButton</WebPartName>
  <WebPartProperties>
    <form version="2">
      <category name="Sign out button settings">
        <properties>
          <visible>True</visible>
        </properties>
      </category>
      <field allowempty="true" column="ShowAsLink" columntype="boolean" guid="da67ee81-61e9-483a-8af3-3ba2693f7e95" publicfield="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>false</defaultvalue>
          <fieldcaption>Show as link</fieldcaption>
          <fielddescription>
            <![CDATA[If enabled, the sign out functionality will be provided by a link instead of a button.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field allowempty="true" column="SignOutText" columnsize="100" columntype="text" guid="184d035a-d84f-46b3-b3c7-2c5bb7ce63ff" publicfield="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Button text for sign out</fieldcaption>
          <fielddescription>
            <![CDATA[Sets the text caption of the sign out button or link.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="RedirectToURL" columnsize="500" columntype="text" guid="cda19ba8-8659-488e-986d-77d2932fbe16" publicfield="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Redirect to URL</fieldcaption>
          <fielddescription>
            <![CDATA[Specifies the URL of the page where users should be redirected after they sign out. For example: ~/home.aspx]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field column="ShowOnlyWhenAuthenticated" columntype="boolean" guid="cd8afc00-9530-47bf-9e3d-cfa287b5c451" publicfield="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>true</defaultvalue>
          <fieldcaption>Show only when authenticated</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates if the sign out button should be hidden for public (unauthenticated) users. Disable this property if you also wish to use the web part to provide a sign in button.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field allowempty="true" column="SkinID" columnsize="200" columntype="text" guid="898c93bd-331d-4ea9-9310-fd32d86ad643" publicfield="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Skin ID</fieldcaption>
          <fielddescription>{$documentation.webpartproperties.skinid$}</fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <category name="Sign in button settings">
        <properties>
          <visible>True</visible>
        </properties>
      </category>
      <field allowempty="true" column="SignInText" columnsize="100" columntype="text" guid="0881e9e8-e26e-4355-b57a-11de2abdbf14" publicfield="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Button text for sign in</fieldcaption>
          <fielddescription>
            <![CDATA[Sets the text caption of the sign in button or link.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="SignInPageUrl" columnsize="100" columntype="text" guid="dd689bd3-0676-460b-becb-e1c1e832e8df" publicfield="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Sign in page path</fieldcaption>
          <fielddescription>
            <![CDATA[Can be used to set the path of a logon page. Users will be redirected to this page when they click the sign in button (or link).]]>
          </fielddescription>
        </properties>
        <settings>
          <AllowSetPermissions>False</AllowSetPermissions>
          <controlname>selectsinglepath</controlname>
        </settings>
      </field>
      <field allowempty="true" column="ReturnPath" columnsize="100" columntype="text" guid="fa8fc46f-2ed5-408f-8f37-7d9d2a7620fc" publicfield="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Return page path</fieldcaption>
          <fielddescription>
            <![CDATA[Can be used to set the path of a page where users will be redirected after they successfully log in. When the sign in button is clicked, the path of this page is passed to the logon page using the ReturnUrl query string parameter.]]>
          </fielddescription>
        </properties>
        <settings>
          <AllowSetPermissions>False</AllowSetPermissions>
          <controlname>selectsinglepath</controlname>
        </settings>
      </field>
    </form>
  </WebPartProperties>
  <WebPartSkipInsertProperties>False</WebPartSkipInsertProperties>
  <WebPartType>0</WebPartType>
</cms.webpart>