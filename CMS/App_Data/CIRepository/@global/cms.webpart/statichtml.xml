<?xml version="1.0" encoding="utf-8"?>
<cms.webpart>
  <WebPartCategoryID>
    <CodeName>Text</CodeName>
    <GUID>68d8e65b-a01c-49ae-b9b6-7580bbce4b09</GUID>
    <ObjectType>cms.webpartcategory</ObjectType>
  </WebPartCategoryID>
  <WebPartCSS>
<![CDATA[


]]>
  </WebPartCSS>
  <WebPartDescription>
    <![CDATA[This web part allows developers to enter HTML content using a WYSIWYG editor and place it onto the page. Unlike with the Editable text web part, page editors cannot modify the content. The static content entered into the web part is displayed on the live site.]]>
  </WebPartDescription>
  <WebPartDisplayName>Static HTML</WebPartDisplayName>
  <WebPartDocumentation />
  <WebPartFileName>Text/statichtml.ascx</WebPartFileName>
  <WebPartGUID>22c87868-6572-46eb-b517-dab4cc97ee78</WebPartGUID>
  <WebPartIconClass>icon-w-static-html</WebPartIconClass>
  <WebPartName>statichtml</WebPartName>
  <WebPartProperties>
    <form version="2">
      <category name="Content">
        <properties>
          <visible>True</visible>
        </properties>
      </category>
      <field allowempty="true" column="Text" columntype="longtext" guid="040fbee7-4d9f-4c50-8846-fdd62daf7e61" publicfield="false" resolvedefaultvalue="False" visibility="none" visible="true">
        <properties>
          <fieldcaption>Text</fieldcaption>
          <fielddescription>
            <![CDATA[Used to enter the content that will be displayed. The WYSIWYG editor can be used to format the text or insert various types of objects.]]>
          </fielddescription>
        </properties>
        <settings>
          <Autoresize_Hashtable>True</Autoresize_Hashtable>
          <controlname>HtmlAreaControl</controlname>
          <Dialogs_Content_Hide>False</Dialogs_Content_Hide>
          <Dialogs_Content_Site>##all##</Dialogs_Content_Site>
          <Dialogs_Libraries_Site>##all##</Dialogs_Libraries_Site>
          <Height>250</Height>
          <HeightUnitType>PX</HeightUnitType>
          <MediaDialogConfiguration>True</MediaDialogConfiguration>
          <ShowAddStampButton>False</ShowAddStampButton>
          <Width>500</Width>
          <WidthUnitType>PX</WidthUnitType>
        </settings>
      </field>
      <field allowempty="true" column="ResolveDynamicControls" columntype="boolean" guid="29e28611-c933-4523-8dba-a8ba1fbe87be" publicfield="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>true</defaultvalue>
          <fieldcaption>Resolve dynamic controls</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates if controls inserted into the HTML content such as BizForms, polls, etc. should be resolved. If disabled, the source expressions of any controls will be displayed.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
    </form>
  </WebPartProperties>
  <WebPartSkipInsertProperties>False</WebPartSkipInsertProperties>
  <WebPartType>0</WebPartType>
</cms.webpart>