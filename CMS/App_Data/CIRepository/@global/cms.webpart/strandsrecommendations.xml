<?xml version="1.0" encoding="utf-8"?>
<cms.webpart>
  <WebPartCategoryID>
    <CodeName>StrandsRecommender</CodeName>
    <GUID>3dc8216f-0c6e-4709-a2b3-3e071ab6b303</GUID>
    <ObjectType>cms.webpartcategory</ObjectType>
  </WebPartCategoryID>
  <WebPartDefaultValues>
    <form />
  </WebPartDefaultValues>
  <WebPartDescription>
    <![CDATA[Displays product recommendations provided by the Strands Recommender service. The web part inserts one Strands Recommender widget onto the page.]]>
  </WebPartDescription>
  <WebPartDisplayName>Strands recommendations</WebPartDisplayName>
  <WebPartFileName>StrandsRecommender/StrandsRecommendations.ascx</WebPartFileName>
  <WebPartGUID>e8984cbd-4643-4cc2-9b38-baae8fb2403b</WebPartGUID>
  <WebPartIconClass>icon-w-strands-recommendations</WebPartIconClass>
  <WebPartName>StrandsRecommendations</WebPartName>
  <WebPartProperties>
    <form version="2">
      <category name="strands.templatecategorycaption">
        <properties>
          <caption>{$strands.templatecategorycaption$}</caption>
          <visible>True</visible>
        </properties>
      </category>
      <field column="RecommendationTemplateIdentifier" columnsize="200" columntype="text" guid="cacbfe3c-9b43-4e36-9ae6-8650dd270a8d" publicfield="false" visible="true">
        <properties>
          <fieldcaption>{$strands.choosetemplate$}</fieldcaption>
          <fielddescription>{$strands.choosetemplate.description$}</fielddescription>
        </properties>
        <settings>
          <controlname>strandswebtemplateselector</controlname>
        </settings>
      </field>
      <field allowempty="true" column="CustomTransformation" columnsize="200" columntype="text" guid="724c8464-c120-4a65-9daa-b043b4dc5c1c" publicfield="false" resolvedefaultvalue="False" visible="true">
        <properties>
          <fieldcaption>{$strands.customlayout$}</fieldcaption>
          <fielddescription>{$strands.customtransformation.description$}</fielddescription>
        </properties>
        <settings>
          <controlname>selecttransformation</controlname>
          <WhereCondition>
            <![CDATA[TransformationType = 'jQuery']]>
          </WhereCondition>
        </settings>
      </field>
    </form>
  </WebPartProperties>
  <WebPartResourceID>
    <CodeName>CMS.StrandsRecommender</CodeName>
    <GUID>1235e27c-5b04-4024-9032-d10ea62cafbe</GUID>
    <ObjectType>cms.resource</ObjectType>
  </WebPartResourceID>
  <WebPartSkipInsertProperties>False</WebPartSkipInsertProperties>
  <WebPartType>0</WebPartType>
</cms.webpart>