<?xml version="1.0" encoding="utf-8"?>
<cms.webpart>
  <WebPartCategoryID>
    <CodeName>Layouts</CodeName>
    <GUID>d5cb0ae0-d055-437e-829f-e2c09a7bdce3</GUID>
    <ObjectType>cms.webpartcategory</ObjectType>
  </WebPartCategoryID>
  <WebPartDescription>
    <![CDATA[This web part provides an AJAX tab layout. Each tab defines a separate web part zone, so any type of content may be added. Users can click on the tabs to select which content should be displayed.]]>
  </WebPartDescription>
  <WebPartDisplayName>Tabs layout</WebPartDisplayName>
  <WebPartDocumentation>
<![CDATA[
To&nbsp;insert content into the&nbsp;tablayout, edit the&nbsp;page on the <strong>Design</strong> tab. Here you can&nbsp;select individual tabs and add any web parts that you need into the corresponding zones.<br />
<br />
For more information, see <a href="~/CMSPages/GetDocLink.ashx?link=layout_webparts" target="_blank">Working with layout web parts</a>.
]]>
  </WebPartDocumentation>
  <WebPartFileName>Layouts/Tabs.ascx</WebPartFileName>
  <WebPartGUID>275728f5-fe2a-4d96-976a-9f84446c3878</WebPartGUID>
  <WebPartIconClass>icon-w-tabs-layout</WebPartIconClass>
  <WebPartName>TabsLayout</WebPartName>
  <WebPartProperties>
    <form version="2">
      <category name="Tabs settings">
        <properties>
          <caption>Layout settings</caption>
          <visible>True</visible>
        </properties>
      </category>
      <field column="Tabs" columntype="integer" guid="12179509-969d-4f4b-886d-7bc03e29a4c5" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>2</defaultvalue>
          <fieldcaption>Number of tabs</fieldcaption>
          <fielddescription>
            <![CDATA[Sets the number of tabs that will be displayed in the layout.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="TabHeaders" columntype="longtext" guid="fefe2a4d-0dcb-485d-bef6-473cb12cc477" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Tab headers</fieldcaption>
          <fielddescription>
            <![CDATA[Specifies the names used for the tab titles. Each tab name must be defined on a new line. Any additional tabs that are not named will use the following format: "Tab <number>"]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textareacontrol</controlname>
          <FilterMode>False</FilterMode>
          <IsTextArea>True</IsTextArea>
          <Wrap>False</Wrap>
        </settings>
      </field>
      <field allowempty="true" column="ActiveTabIndex" columntype="integer" guid="eb6e81ae-06b4-4d41-939b-deb3ddfa2977" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>1</defaultvalue>
          <fieldcaption>Active tab index</fieldcaption>
          <fielddescription>
            <![CDATA[Sets the index of the tab that will be selected by default when the page is loaded. The index of the first tab is 1.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="HideEmptyTabs" columntype="boolean" guid="2e43cc5c-45f1-4f86-912b-9573cb69ac41" publicfield="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>false</defaultvalue>
          <fieldcaption>Hide empty tabs</fieldcaption>
          <fielddescription>
            <![CDATA[If checked, all tabs which contain no visible web parts will be hidden.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field allowempty="true" column="HideIfNoTabsVisible" columntype="boolean" guid="c78efb1f-bf44-4374-bfac-0c8879f23cbc" publicfield="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>true</defaultvalue>
          <fieldcaption>Hide if no tabs visible</fieldcaption>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field allowempty="true" column="AllowDesignMode" columntype="boolean" guid="ef9b34a4-c891-4ace-aac8-ddab9c7caec1" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>true</defaultvalue>
          <fieldcaption>Allow design mode</fieldcaption>
          <fielddescription>
            <![CDATA[{$documentation.webpartproperties.allowdesignmode$}]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <category name="Tabs design">
        <properties>
          <visible>True</visible>
        </properties>
      </category>
      <field allowempty="true" column="TabStripPlacement" columnsize="100" columntype="text" guid="b95f0f85-3a82-4818-a7fa-bae758fc0c9a" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>top</defaultvalue>
          <fieldcaption>Tab strip placement</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates if the tab strip (menu) should be located above the content of the layout (Top) or below (Bottom).]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>radiobuttonscontrol</controlname>
          <Options>
<![CDATA[
top;Top
bottom;Bottom
topright;Top right
bottomright;Bottom right
]]>
          </Options>
          <RepeatDirection>vertical</RepeatDirection>
        </settings>
      </field>
      <field allowempty="true" column="Width" columnsize="100" columntype="text" guid="fa18ef7d-a667-4b46-b86c-4adec1519091" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Width</fieldcaption>
          <fielddescription>
            <![CDATA[Sets the width of the tab content area. It must be specified as a CSS style value, e.g. 200px or 50%.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="Height" columnsize="100" columntype="text" guid="0f01df8a-feb6-49e3-80a5-7afe3f276254" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Height</fieldcaption>
          <fielddescription>
            <![CDATA[Sets the height of the tab content area. It must be specified as a CSS style value, e.g. 200px or 50%.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="TabsCSSClass" columnsize="100" columntype="text" guid="29b33b85-201a-4c82-8c92-c0e6128c5299" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Tabs CSS class</fieldcaption>
          <fielddescription>
            <![CDATA[Sets the name of the CSS class from the used stylesheet that will be applied to the entire tab layout. This overrides the styles that are applied by default.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="Scrollbars" columnsize="100" columntype="text" guid="7baf4170-8f21-4bf4-aee6-5c109a9719c6" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>auto</defaultvalue>
          <fieldcaption>Scrollbars</fieldcaption>
          <fielddescription>
            <![CDATA[Configures the scrollbar settings for the tab content area.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>dropdownlistcontrol</controlname>
          <Options>
            <![CDATA[<item value="auto" text="Auto" /><item value="none" text="None" /><item value="horizontal" text="Horizontal" /><item value="vertical" text="Vertical" /><item value="both" text="Both" />]]>
          </Options>
        </settings>
      </field>
    </form>
  </WebPartProperties>
  <WebPartSkipInsertProperties>True</WebPartSkipInsertProperties>
  <WebPartType>6</WebPartType>
</cms.webpart>