<?xml version="1.0" encoding="utf-8"?>
<cms.webpart>
  <WebPartCategoryID>
    <CodeName>MembershipUsers</CodeName>
    <GUID>a08d8f1a-7771-4832-95e2-2cfc4ca84695</GUID>
    <ObjectType>cms.webpartcategory</ObjectType>
  </WebPartCategoryID>
  <WebPartDescription>
    <![CDATA[Displays a filter that can be used to limit which users should be displayed from the connected data source. It is intended for use in combination with a Users data source web part.]]>
  </WebPartDescription>
  <WebPartDisplayName>Users filter</WebPartDisplayName>
  <WebPartDocumentation />
  <WebPartFileName>Membership/Users/UsersFilter.ascx</WebPartFileName>
  <WebPartGUID>4232cc67-dd48-4ced-aab4-d9eb6755c6ae</WebPartGUID>
  <WebPartIconClass>icon-w-users-filter</WebPartIconClass>
  <WebPartName>UsersFilter</WebPartName>
  <WebPartProperties>
    <form version="2">
      <category name="Filter options">
        <properties>
          <caption>Filter properties</caption>
          <visible>True</visible>
        </properties>
      </category>
      <field allowempty="true" column="SortUserNameLinkText" columnsize="200" columntype="text" guid="fdf00192-3cd4-4b6b-b54c-66f05a60817f" publicfield="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>User name sort link text</fieldcaption>
          <fielddescription>
            <![CDATA[Sets the text caption of the filter's "sort by name" link. This link causes the filter to sort the displayed users according to their user name.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="SortActivityLinkText" columnsize="200" columntype="text" guid="75a6fbed-a996-4a51-b1ee-e535ed65eebd" publicfield="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Activity sort link text</fieldcaption>
          <fielddescription>
            <![CDATA[Sets the text caption of the filter's "sort by activity" link. This link causes the filter to sort the displayed users according to their amount of activity points.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="ButtonText" columnsize="200" columntype="text" guid="6273917b-e165-4113-8f92-441eac7f1af3" publicfield="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>Search</defaultvalue>
          <fieldcaption>Button text</fieldcaption>
          <fielddescription>
            <![CDATA[Sets the text displayed as the caption of the button that applies the filter to the data.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="DisableFilterCaching" columntype="boolean" guid="5453ad02-ddaf-44da-ba25-f7900233b951" publicfield="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>false</defaultvalue>
          <fieldcaption>Disables filter caching</fieldcaption>
          <fielddescription>
            <![CDATA[{$documentation.webpartproperties.disablefiltercaching$}]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
    </form>
  </WebPartProperties>
  <WebPartSkipInsertProperties>False</WebPartSkipInsertProperties>
  <WebPartType>2</WebPartType>
</cms.webpart>