<?xml version="1.0" encoding="utf-8"?>
<cms.webpart>
  <WebPartCategoryID>
    <CodeName>Layouts</CodeName>
    <GUID>d5cb0ae0-d055-437e-829f-e2c09a7bdce3</GUID>
    <ObjectType>cms.webpartcategory</ObjectType>
  </WebPartCategoryID>
  <WebPartDescription>
    <![CDATA[Provides a web part zone that can be used to group multiple web parts or assign additional formatting to them.]]>
  </WebPartDescription>
  <WebPartDisplayName>Web part zone</WebPartDisplayName>
  <WebPartDocumentation>
<![CDATA[
To&nbsp;insert content into the&nbsp;zone, edit the&nbsp;page on the <strong>Design</strong> tab. You can add web parts into the zone using the same approach as&nbsp;for a&nbsp;standard web part zone defined by the layout of the page template.<br />
<br />
For more information, see <a href="~/CMSPages/GetDocLink.ashx?link=layout_webparts" target="_blank">Working with layout web parts</a>.
]]>
  </WebPartDocumentation>
  <WebPartFileName>Layouts/Zone.ascx</WebPartFileName>
  <WebPartGUID>7ca8cf20-8f1a-4752-82f0-08a13f5a0401</WebPartGUID>
  <WebPartIconClass>icon-w-web-part-zone</WebPartIconClass>
  <WebPartName>WebPartZone</WebPartName>
  <WebPartProperties>
    <form version="2">
      <category name="Size">
        <properties>
          <caption>Layout</caption>
          <visible>True</visible>
        </properties>
      </category>
      <field allowempty="true" column="Width" columnsize="100" columntype="text" guid="71645a48-7171-4eeb-adef-6fd860928afd" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Width</fieldcaption>
          <fielddescription>
            <![CDATA[Sets the width of the zone, specified as a CSS style value, e.g. 200px or 50%.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="Height" columnsize="100" columntype="text" guid="2ea38798-d14a-4b5e-800a-c35303db2184" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Height</fieldcaption>
          <fielddescription>
            <![CDATA[Sets the height of the zone, specified as a CSS style value, e.g. 200px or 50%.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="ZoneCSSClass" columnsize="100" columntype="text" guid="498ff7e1-ba99-486e-b43e-9ac814896224" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>CSS class</fieldcaption>
          <fielddescription>
            <![CDATA[Sets the name of the CSS class applied to the content of the zone.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="AllowDesignMode" columntype="boolean" guid="d397af99-2220-4659-ab0e-1de0b6a54724" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>true</defaultvalue>
          <fieldcaption>Allow design mode</fieldcaption>
          <fielddescription>
            <![CDATA[{$documentation.webpartproperties.allowdesignmode$}]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
    </form>
  </WebPartProperties>
  <WebPartSkipInsertProperties>True</WebPartSkipInsertProperties>
  <WebPartType>6</WebPartType>
</cms.webpart>