<?xml version="1.0" encoding="utf-8"?>
<cms.webpart>
  <WebPartCategoryID>
    <CodeName>SyndicationDataSources</CodeName>
    <GUID>e59c95b7-0abc-4049-b9f5-45d0fc2a736f</GUID>
    <ObjectType>cms.webpartcategory</ObjectType>
  </WebPartCategoryID>
  <WebPartDescription>
    <![CDATA[This web part retrieves data from a specified web service and provides it as a data source. The columns contained in the resulting data set depend on the specifics of the given web service. This data may then be displayed by a connected listing web part (such as a Basic repeater).]]>
  </WebPartDescription>
  <WebPartDisplayName>Web service data source</WebPartDisplayName>
  <WebPartDocumentation>
    <![CDATA[You can create a custom web service by adding the necessary code into the sample <strong>~/CMSPages/WebService.asmx </strong>file located in&nbsp;your web project. The web part can then retrieve the data from this web service and provide it as a data source.]]>
  </WebPartDocumentation>
  <WebPartFileName>DataSources/WebServiceDataSource.ascx</WebPartFileName>
  <WebPartGUID>29c231fc-2748-4953-bfd5-3df26f933fff</WebPartGUID>
  <WebPartIconClass>icon-w-web-service-data-source</WebPartIconClass>
  <WebPartName>WebServiceDataSource</WebPartName>
  <WebPartProperties>
    <form version="2">
      <category name="Filter">
        <properties>
          <visible>True</visible>
        </properties>
      </category>
      <field column="WebServiceUrl" columnsize="1000" columntype="text" guid="fd71f2cf-cbc8-4beb-988e-d01235a4fa1a" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Web service URL (.asmx file)</fieldcaption>
          <fielddescription>{$documentation.webpartproperties.webserviceurl$}</fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="WebServiceParameters" columntype="longtext" guid="c2ac39f1-4bda-47d0-9dc1-d296e30be7c8" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Web service parameter</fieldcaption>
          <fielddescription>
            <![CDATA[{$documentation.webpartproperties.webserviceparameters$}]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textareacontrol</controlname>
          <FilterMode>False</FilterMode>
        </settings>
      </field>
      <category name="System settings">
        <properties>
          <visible>True</visible>
        </properties>
      </category>
      <field allowempty="true" column="CacheItemName" columnsize="200" columntype="text" guid="c37d524a-545a-420c-b8df-14da5e6e15db" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Cache item name</fieldcaption>
          <fielddescription>{$documentation.webpartproperties.cacheitemname$}</fielddescription>
        </properties>
        <settings>
          <controlname>textboxcontrol</controlname>
        </settings>
      </field>
      <field allowempty="true" column="CacheMinutes" columntype="integer" guid="0b9d3bc7-7af0-4fb3-8ea2-50a423c83317" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Cache minutes</fieldcaption>
          <fielddescription>{$documentation.webpartproperties.cacheminutes$}</fielddescription>
        </properties>
        <settings>
          <controlname>textboxcontrol</controlname>
        </settings>
      </field>
      <field allowempty="true" column="CacheDependencies" columntype="longtext" guid="14edde76-54a2-4cc4-be57-1c97988c17c3" publicfield="false" spellcheck="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Cache dependencies</fieldcaption>
          <fielddescription>
            <![CDATA[{$documentation.webpartproperties.cachedependencies$}]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>cachedependencies</controlname>
        </settings>
      </field>
    </form>
  </WebPartProperties>
  <WebPartSkipInsertProperties>False</WebPartSkipInsertProperties>
  <WebPartType>1</WebPartType>
</cms.webpart>