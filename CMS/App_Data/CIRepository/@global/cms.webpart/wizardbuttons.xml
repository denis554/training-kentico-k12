        </settings>
      </field>
      <field allowempty="true" column="FinishText" columnsize="200" columntype="text" guid="de17d549-0db8-40cf-9bdf-be0052952c5a" publicfield="false" visibility="none" visible="true">
        <properties>
          <defaultvalue>Finish</defaultvalue>
