          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="FinishImage" columnsize="400" columntype="text" guid="a64a5fef-292a-44f3-a752-5279c1ab84b4" publicfield="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Finish button image</fieldcaption>
          <fielddescription>
            <![CDATA[May be used to select an image that will be added to the finish link. The image is only displayed if the 'Show finish as button' property is disabled. If you wish to display the image without the text link, set the 'Finish button text' property to an empty value.]]>
          </fielddescription>
        </properties>
        <settings>
          <Autoresize_Hashtable>True</Autoresize_Hashtable>
          <controlname>mediaselectioncontrol</controlname>
        </settings>
      </field>
      <field allowempty="true" column="FinishCssClass" columnsize="100" columntype="text" guid="a94054d6-36fc-4f78-be6f-a8bf828509c8" publicfield="false" visibility="none" visible="true">
        <properties>
          <fieldcaption>Finish button CSS class</fieldcaption>
          <fielddescription>
            <![CDATA[Specifies the name of the CSS class applied to the finish button.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
    </form>
  </WebPartProperties>
  <WebPartSkipInsertProperties>True</WebPartSkipInsertProperties>
  <WebPartType>0</WebPartType>
</cms.webpart>